    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 47.</fw>
                <head type="title">XLVII. SOLTAR. T. B.</head>
                <head type="sub">Háláadása az Izraelitáknac, az Istennec soc féle
                    jotéteményiért.</head>
                <pb facs="120"> </pb>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">NO minden népec</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.1">Noh</rdg>
                        </app></l>
                    <l xml:id="l.2">Örvendezzetec,</l>
                    <l xml:id="l.3">Es tapsollyatoc</l>
                    <l xml:id="l.4">Istent áldgyátoc,</l>
                    <l xml:id="l.5">Szép hangossággal</l>
                    <l xml:id="l.6">Es nagy föl szoval.</l>
                    <l rend="level_1" xml:id="l.7">Mert az Ur Isten</l>
                    <l xml:id="l.8">Nagy fölségében,</l>
                    <l xml:id="l.9"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Ö királs<corr>á</corr>ga</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.2">királsága</rdg>
                            <rdg wit="#W3" xml:id="rdg.3">királsaga</rdg>
                        </app></l>
                    <l xml:id="l.10"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Es nagy orsz<corr>á</corr>ga,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.4">országa</rdg>
                            <rdg wit="#W3" xml:id="rdg.5">orszaga</rdg>
                        </app></l>
                    <l xml:id="l.11">Kiterjed meszsze</l>
                    <l xml:id="l.12">Ez egesz földre.</l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.13"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Hatalmunkba vét</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.6">Hatalmünkban [I <hi rend="italic"
                                    >Corrigenda</hi>-jában: Hatalmũkban] </rdg>
                            <rdg wit="#W1" xml:id="rdg.7">vet</rdg>
                        </app></l>
                    <l xml:id="l.14">Nagy soc népeket,</l>
                    <l xml:id="l.15"><app xml:id="app.5">
                            <lem wit="#KK" xml:id="lem.5">Es megholdoltat</lem>
                            <rdg wit="#W2 #W3" xml:id="rdg.8">[A 15–16. sorok felcserélve.]</rdg>
                        </app></l>
                    <l xml:id="l.16"><app xml:id="app.6">
                            <lem wit="#KK" xml:id="lem.6">Soc pogányokat</lem>
                            <rdg wit="#W1" xml:id="rdg.9">pogánokat</rdg>
                            <rdg wit="#W2 #W3" xml:id="rdg.10">[A 15–16. sorok felcserélve.]</rdg>
                        </app></l>
                    <l xml:id="l.17"><app xml:id="app.7">
                            <lem wit="#KK" xml:id="lem.7">Mi nékünc végre</lem>
                            <rdg wit="#W2 #W3" xml:id="rdg.11">[A 17–18. sorok felcserélve.]</rdg>
                        </app></l>
                    <l xml:id="l.18"><app xml:id="app.8">
                            <lem wit="#KK" xml:id="lem.8">Ö nagy ereje.</lem>
                            <rdg wit="#W2 #W3" xml:id="rdg.12">[A 17–18. sorok felcserélve.]</rdg>
                        </app></l>
                    <l rend="level_1" xml:id="l.19">Minket ö fölvött</l>
                    <l xml:id="l.20">Örökévé tött,</l>
                    <l xml:id="l.21">Nékünc engedte</l>
                    <l xml:id="l.22">Ö kegyessége</l>
                    <l xml:id="l.23">Jacobnac tisztét,</l>
                    <l xml:id="l.24">Kit igen szeret.</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.25">Imé az Isten</l>
                    <l xml:id="l.26">Szépen föhnegyen,</l>
                    <l xml:id="l.27">Nagy vigaságban,</l>
                    <l xml:id="l.28">Trombita szokban,</l>
                    <l xml:id="l.29">Urunc fölmegyen</l>
                    <l xml:id="l.30"><app xml:id="app.9">
                            <lem wit="#KK" xml:id="lem.9">Nagy diczöségben.</lem>
                            <rdg wit="#W1" xml:id="rdg.13">düczöségben</rdg>
                        </app></l>
                    <l rend="level_1" xml:id="l.31">Enekellyetec</l>
                    <l xml:id="l.32">Az Ur Istennec</l>
                    <l xml:id="l.33">Zengö verseket</l>
                    <l xml:id="l.34">Szép énekeket</l>
                    <l xml:id="l.35">Ez nagy királnac,</l>
                    <l xml:id="l.36">Mint mi Urunknac.</l>
                </lg>
                <lg xml:id="lg.4">
                    <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                    <l xml:id="l.37">Önéki minden</l>
                    <l xml:id="l.38">Jol énekellyen,</l>
                    <l xml:id="l.39">Mert minden népec</l>
                    <l xml:id="l.40">Néki engednec,</l>
                    <l xml:id="l.41">Királyi székben</l>
                    <l xml:id="l.42">Ül nagy kegyessen.</l>
                    <l rend="level_1" xml:id="l.43">Az fejedelmec</l>
                    <l xml:id="l.44">Ö hozzá gyülnec,</l>
                    <l xml:id="l.45">Mint Abrahamnac</l>
                    <l xml:id="l.46"><app xml:id="app.10">
                            <lem wit="#KK" xml:id="lem.10">Istenét áldgyac</lem>
                            <rdg wit="#W2" xml:id="rdg.14">álgyác</rdg>
                        </app></l>
                    <l xml:id="l.47">Alázatossan</l>
                    <l xml:id="l.48">Néki szolgálván.</l>
                </lg>
                <lg xml:id="lg.5">
                    <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                    <l xml:id="l.49">Ez erös Isten</l>
                    <l xml:id="l.50">Ur mindeneken,</l>
                    <l xml:id="l.51">Népét megörzi</l>
                    <l xml:id="l.52">Es jol vezérli,</l>
                    <l xml:id="l.53"><app xml:id="app.11">
                            <lem wit="#KK" xml:id="lem.11">Nagy diczösege,</lem>
                            <rdg wit="#W1" xml:id="rdg.15">düczösege</rdg>
                        </app></l>
                    <l xml:id="l.54">Kinec nincz vége.</l>
                </lg>
            </div>
        </body>
