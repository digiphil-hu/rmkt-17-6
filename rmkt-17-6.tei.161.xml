   <text>
      <body>
         <div type="poem" xml:id="d.1" xml:lang="hu">
            <fw type="originalNum">[RMKT XVII/6:] 161.</fw>
            <head type="title"><app xml:id="app.1">
                  <lem wit="#KK" xml:id="lem.1"><supplied>AZ VRNAC IMADSÁGA</supplied></lem>
                  <rdg wit="#W2" xml:id="rdg.1">Az Mi Atyánc.</rdg>
               </app></head>
            <head type="notation">Az 117. Soltarnac Franciai notájara.</head>
            <lg xml:id="lg.1">
               <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
               <l xml:id="l.1">Mi Atyánc ô kegyes Isten,</l>
               <l xml:id="l.2">Ki vagy az magas mennyekben,</l>
               <l xml:id="l.3">Szenteltesséc neved szivvel,</l>
               <l xml:id="l.4">Az te országod jöyjön el,</l>
               <l xml:id="l.5">Te akaratod meglegyen,</l>
               <l xml:id="l.6">Ez földön miképpen menyben.</l>
            </lg>
            <lg xml:id="lg.2">
               <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
               <l xml:id="l.7">Minden napi kenyerünket</l>
               <l xml:id="l.8">Addmeg boczasd meg vétkünket,</l>
               <l xml:id="l.9">Az mint miis megboczátunc,</l>
               <l xml:id="l.10">Azoknac kiktöl bántatunc</l>
               <l xml:id="l.11"><app xml:id="app.2">
                     <lem wit="#KK" xml:id="lem.2">Es ne vigy az kisértetben,</lem>
                     <rdg wit="#W2" xml:id="rdg.2">késértetben</rdg>
                  </app></l>
               <l xml:id="l.12">De szabadits gonosz ellen.</l>
            </lg>
            <lg xml:id="lg.3">
               <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
               <l xml:id="l.13">Mert tiéd Uram az ország,</l>
               <l xml:id="l.14">Tied minden hatalmasság,</l>
               <l xml:id="l.15">Megadhatsz azért minékünc,</l>
               <l xml:id="l.16">Mindent az mit töled kerünc,</l>
               <l xml:id="l.17">Tied az diczöség Amen,</l>
               <l xml:id="l.18">Most és öröcké ugy legyen.</l>
            </lg>
         </div>
      </body>
