    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 99.</fw>
                <head type="title">XCIX. SOLTAR. T. B.</head>
                <head type="sub">Intés az Urnac diczéretire és imádására.</head>
                <pb facs="228"> </pb>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">AZ Ur országol</l>
                    <l xml:id="l.2">Es regnál nagy jol,</l>
                    <l xml:id="l.3">Az nép megrémül,</l>
                    <l xml:id="l.4">Hogy ö ottfön ül</l>
                    <l xml:id="l.5">Az Kérubimon,</l>
                    <l xml:id="l.6">Ki elött nagyon</l>
                    <l xml:id="l.7">Félnec és rettegnec,</l>
                    <l xml:id="l.8">Ez földön mindenec.</l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.9">Nagy az Ur Isten</l>
                    <l xml:id="l.10">Ö erejében,</l>
                    <l xml:id="l.11">Az Sion hegyen</l>
                    <l xml:id="l.12">Minden népeken</l>
                    <l xml:id="l.13">Vagyon hatalma.</l>
                    <l xml:id="l.14">Öt minden áldgya,</l>
                    <l xml:id="l.15">Mert nagy az ö neve</l>
                    <l xml:id="l.16"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">Es diczö szentsége.</lem>
                            <rdg wit="#W1" xml:id="rdg.1">düczö</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.17"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Ez erös Urn<corr>a</corr>c</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.2">Urnac</rdg>
                            <rdg wit="#W3" xml:id="rdg.3">Urnác</rdg>
                        </app></l>
                    <l xml:id="l.18">Mint jo királynac</l>
                    <l xml:id="l.19">Nem kell hamisság,</l>
                    <l xml:id="l.20">Czac kell igasság.</l>
                    <l xml:id="l.21">Jo itéletöt</l>
                    <l xml:id="l.22">Az Jácobval töt,</l>
                    <l xml:id="l.23">Kit ö igen szeret,</l>
                    <l xml:id="l.24">Mint választott népet.</l>
                </lg>
                <lg xml:id="lg.4">
                    <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                    <l xml:id="l.25">Istent áldgyatoc,</l>
                    <l xml:id="l.26">Magaztallyátoc,</l>
                    <l xml:id="l.27">Haytsatoc térdöt</l>
                    <l xml:id="l.28">Sámolya elöt,</l>
                    <l xml:id="l.29">Mert az szentséges</l>
                    <l xml:id="l.30">Aron és Moses,</l>
                    <l xml:id="l.31">Az ö szent papjai</l>
                    <l xml:id="l.32">Könyörgöttec néki:</l>
                </lg>
                <lg xml:id="lg.5">
                    <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                    <l xml:id="l.33">Es az Samuel</l>
                    <l xml:id="l.34">Könyörgésével</l>
                    <l xml:id="l.35">Istent keresé</l>
                    <l xml:id="l.36">Nagy szükségébe.</l>
                    <l xml:id="l.37">Es kérésekben</l>
                    <l xml:id="l.38">Meghallá Isten,</l>
                    <l xml:id="l.39">Megadá nékiec</l>
                    <l xml:id="l.40">Az mit töle kértec.</l>
                </lg>
                <lg xml:id="lg.6">
                    <fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
                    <l xml:id="l.41">Fön az fölhöben</l>
                    <l xml:id="l.42">Oszlop képében,</l>
                    <l xml:id="l.43">Ököt vezérlé</l>
                    <l xml:id="l.44">Az puszta helybe,</l>
                    <l xml:id="l.45">Kic ö törvényét</l>
                    <l xml:id="l.46">Es szent igéjét</l>
                    <l xml:id="l.47">Hiven megöriztéc,</l>
                    <l xml:id="l.48">Frigyét kedvellettéc.</l>
                </lg>
                <lg xml:id="lg.7">
                    <fw corresp="Szenci Molnár Albert" type="lg">7 </fw>
                    <l xml:id="l.49">Meghallád Isten</l>
                    <l xml:id="l.50">Ö keresekben,</l>
                    <l xml:id="l.51">Hozzájoc térél.</l>
                    <l xml:id="l.52">Es megengedél</l>
                    <l xml:id="l.53">Kegyességedböl.</l>
                    <l xml:id="l.54">De hogy bünökböl</l>
                    <l xml:id="l.55">Ök ki nem térénec</l>
                    <l xml:id="l.56"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Megbüntett<corr>et</corr>énec.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.4">Megbüntettenec</rdg>
                            <rdg wit="#W3" xml:id="rdg.5">Megbüntetténec</rdg>
                        </app><note type="ciritc">Szótagszám!</note></l>
                </lg>
                <lg xml:id="lg.8">
                    <fw corresp="Szenci Molnár Albert" type="lg">8 </fw>
                    <l xml:id="l.57">Aldgyátoc ötet</l>
                    <l xml:id="l.58"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Mint Iste<corr>n</corr>ünket,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.6">Istenünket</rdg>
                            <rdg wit="#W3" xml:id="rdg.7">Istennünket</rdg>
                        </app></l>
                    <l xml:id="l.59">Térdet haytsatoc</l>
                    <l xml:id="l.60">Es imádgyátoc,</l>
                    <l xml:id="l.61">Az Sion hegyén</l>
                    <l xml:id="l.62">Ö lako helyén,</l>
                    <l xml:id="l.63">Diczértetic itten,</l>
                    <l xml:id="l.64">Mert szent az Ur Isten.</l>
                </lg>
            </div>
