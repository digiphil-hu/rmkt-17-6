	<text>
		<body>
			<div type="poem" xml:id="d.1" xml:lang="hu">
				<fw type="originalNum">[RMKT XVII/6:] 110.</fw>
				<head type="title">CX. SOLTAR. C. M.</head>
				<head type="sub">Jövendöles az Christus országárol.</head>
				<pb facs="260"> </pb>
				<lg xml:id="lg.1">
					<fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
					<l xml:id="l.1">AZ Ur Isten monda az én Uramnac:</l>
					<l xml:id="l.2"><app xml:id="app.1">
							<lem wit="#KK" xml:id="lem.1">Üly az én hatalmomnac jobjâra,</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.1">ülly</rdg>
						</app></l>
					<l xml:id="l.3"><app xml:id="app.2">
							<lem wit="#KK" xml:id="lem.2">Mig ellenségidet, <corr>k</corr>ic rád
								támadnac</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.2">kic</rdg>
							<rdg wit="#W3" xml:id="rdg.3">Kic</rdg>
						</app></l>
					<l xml:id="l.4">Sámoly székül vetem lábad alá.</l>
				</lg>
				<lg xml:id="lg.2">
					<fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
					<l xml:id="l.5">Az Ur az Sionbol küldi páltzádat,</l>
					<l xml:id="l.6">Birodalmodnac erösségére,</l>
					<l xml:id="l.7">Ellenség közt mutasd országlásodat,</l>
					<l xml:id="l.8">Uralkodgyál az népnec közötte.</l>
				</lg>
				<lg xml:id="lg.3">
					<fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
					<l xml:id="l.9"><app xml:id="app.3">
							<lem wit="#KK" xml:id="lem.3">Diczöségére az te szentségednec,</lem>
							<rdg wit="#W1" xml:id="rdg.4">Düczöségére</rdg>
						</app></l>
					<l xml:id="l.10">Az nép örvendez gyözödelmeden,</l>
					<l xml:id="l.11">Olly soc fiaid tenéked születnec,</l>
					<l xml:id="l.12">Mint az haynali harmat az földen.</l>
				</lg>
				<lg xml:id="lg.4">
					<fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
					<l xml:id="l.13"><app xml:id="app.4">
							<lem wit="#KK" xml:id="lem.4">Mert az Ur Isten meg eskütt teneked,</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.5">téneked</rdg>
						</app></l>
					<l xml:id="l.14">Mellyet meg nem bán soha öröcké,</l>
					<l xml:id="l.15">Melchisedec rendi szerint (ezt értsed)</l>
					<l xml:id="l.16">Te vagy az fö Pap most és öröcké.</l>
				</lg>
				<lg xml:id="lg.5">
					<fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
					<l xml:id="l.17">Az Ur az ki ül az te jobkezeden,</l>
					<l xml:id="l.18">Ha megharagszic egykor valoban,</l>
					<l xml:id="l.19">Az hatalmas királyokat erössen</l>
					<l xml:id="l.20">Hatalmával megrontya legottan.</l>
				</lg>
				<lg xml:id="lg.6">
					<fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
					<l xml:id="l.21">Az pogányokon itéletit tartya,</l>
					<l xml:id="l.22">Megtölti az földet holttesteckel,</l>
					<l xml:id="l.23"><app xml:id="app.5">
							<lem wit="#KK" xml:id="lem.5">Ellenségidnec fejeket
								megron<corr>t</corr>ya,</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.6">megrontya</rdg>
							<rdg wit="#W3" xml:id="rdg.7">megronttya</rdg>
						</app></l>
					<l xml:id="l.24">Országa kihat ez földre széllel.</l>
				</lg>
				<lg xml:id="lg.7">
					<fw corresp="Szenci Molnár Albert" type="lg">7 </fw>
					<l xml:id="l.25">Az uton iszic ö az folyamatbol,</l>
					<l xml:id="l.26"><app xml:id="app.6">
							<lem wit="#KK" xml:id="lem.6">Kinec vize foly nagy harsagással,</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.8">folly</rdg>
						</app></l>
					<l xml:id="l.27">Ennekokaért ö nagy hatalmábol</l>
					<l xml:id="l.28">Fejét fölemeli méltossággal.</l>
				</lg>
			</div>
		</body>
