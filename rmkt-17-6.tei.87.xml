    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 87.</fw>
                <head type="title">LXXXVII. SOLTAR. T. B.</head>
                <head type="sub">Öröme az Sido népnec az uy templomon.</head>
                <pb facs="207"> </pb>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">AZ Ur Isten az ö lakohaylékát</l>
                    <l xml:id="l.2">Az ö szentséges hegyén helyheti,</l>
                    <l xml:id="l.3">Es az Siont ö inkàb szereti,</l>
                    <l xml:id="l.4"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">Hogynem Jacobnak akarmelly sátorát.</lem>
                            <rdg wit="#W1" xml:id="rdg.1">Jácobnac</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.5">Te felöled nagy diczéretec lesznec</l>
                    <l xml:id="l.6">Oh Isten városa! Egyptomat,</l>
                    <l xml:id="l.7">Mond Isten, és az Babyloniát</l>
                    <l xml:id="l.8">Azokhoz tudom,<note type="critic" xml:id="note.1">Vö.
                                <persName>Károli <idno corresp="Károli Gáspár" type="ITIdata"
                                    >Q248162</idno></persName>: azoc közzé számlálom.</note> kic
                        engem esmernec.</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.9">Az Philistéusrol még igy beszélnec,</l>
                    <l xml:id="l.10">Az Tyrusrol és az Szereczenröl,</l>
                    <l xml:id="l.11">Ecképpen szolnac és egyéb népröl:</l>
                    <l xml:id="l.12">Ezec az Sion hegyén születtenec.</l>
                </lg>
                <lg xml:id="lg.4">
                    <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                    <l xml:id="l.13"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Az Isten jol megerös<corr>é</corr>ti
                                öket,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.2">megeröséti</rdg>
                            <rdg wit="#W3" xml:id="rdg.3">megeröseti</rdg>
                        </app></l>
                    <l xml:id="l.14"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Mikor népeit
                                öszvesz<corr>á</corr>mlállya,</lem>
                            <rdg wit="#W1" xml:id="rdg.4">öszve számlálja</rdg>
                            <rdg wit="#W2" xml:id="rdg.5">öszveszámlállya</rdg>
                            <rdg wit="#W3" xml:id="rdg.6">öszveszamlállya</rdg>
                        </app></l>
                    <l xml:id="l.15">Ackor ezec felölis azt mondgya:</l>
                    <l xml:id="l.16"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Ez népis <corr>a</corr>z szent Sionrôl
                                születet.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.7">az</rdg>
                            <rdg wit="#W3" xml:id="rdg.8">áz</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.5">
                    <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                    <l xml:id="l.17">Azért ott énekelnec szép éneket,</l>
                    <l xml:id="l.18">Mond az Isten, és itt örvendeznec,</l>
                    <l xml:id="l.19"><app xml:id="app.5">
                            <lem wit="#KK" xml:id="lem.5">Mert én diczöségére ez helynec</lem>
                            <rdg wit="#W1" xml:id="rdg.9">düczöségére</rdg>
                        </app></l>
                    <l xml:id="l.20">Támasztoc nagy szép élö kutfejeket.</l>
                </lg>
            </div>
