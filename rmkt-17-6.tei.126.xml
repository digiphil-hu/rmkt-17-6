    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 126.</fw>
                <head type="title">CXXVI. SOLTAR. T. B. </head>
                <head type="sub"><app xml:id="app.1">
                        <lem wit="#KK" xml:id="lem.1">Háláadás az Babyloniai fogságbol valo
                                megsz<corr>a</corr>badulásért.</lem>
                        <rdg wit="#W1 #W2" xml:id="rdg.1">megszabadulásért</rdg>
                        <rdg wit="#W3" xml:id="rdg.2">megszábadulásért</rdg>
                    </app></head>
                <pb facs="296"/>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">MIkor az Siont az Isten</l>
                    <l xml:id="l.2">Fogságbol kihozá hiven,</l>
                    <l xml:id="l.3">Ugy mentünc mint egy álomban,</l>
                    <l xml:id="l.4"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Vig nevetség volt
                                száj<corr>u</corr>nkban.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.3">szájunkban</rdg>
                            <rdg wit="#W3" xml:id="rdg.4">szájúnkban</rdg>
                        </app></l>
                    <l rend="level_1" xml:id="l.5">Diczekedtünc az mi nyelvünckel,</l>
                    <l xml:id="l.6">Es éneklettünc nagy örömmel,</l>
                    <l xml:id="l.7">Az pogány közt minden mondta:</l>
                    <l xml:id="l.8">Ez nyilván az Isten dolga.</l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.9">Az Ur mutatá hatalmát,</l>
                    <l xml:id="l.10">Mivélünc tön nagy czudákat,</l>
                    <l xml:id="l.11">Azért öt diczérjüc itten,</l>
                    <l xml:id="l.12">Örvendezzünc sziveinkben.</l>
                    <l rend="level_1" xml:id="l.13">Hozdki Ur Isten az többitis,</l>
                    <l xml:id="l.14">Vesd végét ö fogságoknakis,</l>
                    <l xml:id="l.15">Mint az erös zugo széllel</l>
                    <l xml:id="l.16">Mind ez föld megszárad széllyel.</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.17">Az kic nagy könyhullatással,</l>
                    <l xml:id="l.18">Buzát vetnec nagy bánattal,</l>
                    <l xml:id="l.19"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Arat<corr>á</corr>sk<corr>o</corr>r azoc
                                széllel</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.5">Aratáskor</rdg>
                            <rdg wit="#W3" xml:id="rdg.6">Arataskór</rdg>
                        </app></l>
                    <l xml:id="l.20">Aratnac nagy vig örömmel.</l>
                    <l rend="level_1" xml:id="l.21">Sirva mennec ki az vetésre,</l>
                    <l xml:id="l.22">Az magot hintic keseregve,</l>
                    <l xml:id="l.23">De az jo kévéket osztán,</l>
                    <l xml:id="l.24"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Béhordgyác nagy vigaságban.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.7">vigasságban</rdg>
                        </app></l>
                </lg>
            </div>
        </body>
    </text>
