    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 190.</fw>
                <head type="title"><supplied>VTON JÁRÓNAC ÉNEKE JO UTÉRT</supplied></head>
                <head type="notation">Notaia: Az én életemnec minden etc.</head>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">Emberi nemzetnec te igaz örzöje,</l>
                    <l xml:id="l.2"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">Mennec és az földnec n<corr>e</corr>mes
                                teremtöje,</lem>
                            <rdg wit="#W1" xml:id="rdg.1">nimes</rdg>
                        </app></l>
                    <l xml:id="l.3">Minden állatoknac kegyes eltetöje,</l>
                    <l xml:id="l.4">Az utonjáróknac irgalmas vezére.</l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.5">Semmi félelem nincz te örzésed alat,</l>
                    <l xml:id="l.6">Senki bátorsaggal az kivöl nem járhat,</l>
                    <l xml:id="l.7">Szent Angyalid által vezerled utokat,</l>
                    <l xml:id="l.8"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Azoknac kic read bizzac
                                ömag<corr>o</corr>kat.</lem>
                            <rdg wit="#W1" xml:id="rdg.2">ömagökat</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.9">Kegyelmességedböl ez uton járásom</l>
                    <l xml:id="l.10">Legyen jo szerenczés minden faratságom</l>
                    <l xml:id="l.11"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Hogy az hova megyec mostani
                                    ut<corr>a</corr>mon</lem>
                            <rdg wit="#W1" xml:id="rdg.3">utatmon</rdg>
                        </app></l>
                    <l xml:id="l.12">Mindennemü dolgom végezzem jo modon.</l>
                </lg>
                <lg xml:id="lg.4">
                    <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                    <l xml:id="l.13">Térhessec meg onnét nagy jo egésségben,</l>
                    <l xml:id="l.14">Hálákat adhassac azockal egyemben,</l>
                    <l xml:id="l.15"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Az kic örvendeznec én
                                m<corr>e</corr>gtéresemen</lem>
                            <rdg wit="#W1" xml:id="rdg.4">mégtéresemen</rdg>
                        </app></l>
                    <l xml:id="l.16">Add meg ezt énnékem kegyes Isten, Amen.</l>
                </lg>
            </div>
        </body>
