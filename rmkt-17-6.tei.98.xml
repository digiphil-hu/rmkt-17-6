	<text>
		<body>
			<div type="poem" xml:id="d.1" xml:lang="hu">
				<fw type="originalNum">[RMKT XVII/6:] 98.</fw>
				<head type="title">XCVIII. SOLTAR. T. B. 66.118.</head>
				<head type="sub"><app xml:id="app.1">
						<lem wit="#KK" xml:id="lem.1">Intés az Christusnac
							dicz<corr>é</corr>retire.</lem>
						<rdg wit="#W1 #W2" xml:id="rdg.1">diczéretire</rdg>
						<rdg wit="#W3" xml:id="rdg.2">diczeretire</rdg>
					</app></head>
				<head type="notation">Az LXVI. Soltar notajára.</head>
				<lg xml:id="lg.1">
					<fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
					<l xml:id="l.1">ENekellyetec uy éneket</l>
					<l xml:id="l.2">Az Ur Istennec örömvel,</l>
					<l xml:id="l.3">Mert nagy czudákat czelekedet</l>
					<l xml:id="l.4">Erös karja erejével.</l>
					<l rend="level_1" xml:id="l.5"><app xml:id="app.2">
							<lem wit="#KK" xml:id="lem.2">Mivelünc az ö idvösségét</lem>
							<rdg wit="#W1" xml:id="rdg.3">üdvösségét</rdg>
						</app></l>
					<l xml:id="l.6">Kétségnélkül megérteté,</l>
					<l xml:id="l.7">Igasságát és kegyességét</l>
					<l xml:id="l.8">Minden népnec kijelenté!</l>
				</lg>
				<lg xml:id="lg.2">
					<fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
					<l xml:id="l.9">Meggondolá irgalmasságát,</l>
					<l xml:id="l.10">Es kegyességét tekinté,</l>
					<l xml:id="l.11"><app xml:id="app.3">
							<lem wit="#KK" xml:id="lem.3">Es az ö nagy hivséges
								volt<corr>á</corr>t</lem>
							<rdg wit="#W1" xml:id="rdg.4">hüséges</rdg>
							<rdg wit="#W1 #W2" xml:id="rdg.5">voltát</rdg>
							<rdg wit="#W3" xml:id="rdg.6">voltat</rdg>
						</app></l>
					<l xml:id="l.12">Az Izraelhez térité.</l>
					<l rend="level_1" xml:id="l.13"><app xml:id="app.4">
							<lem wit="#KK" xml:id="lem.4">Az nékünc küldött idvösséget</lem>
							<rdg wit="#W1" xml:id="rdg.7">üdvességet</rdg>
						</app></l>
					<l xml:id="l.14">Ez egész föld kerekségen</l>
					<l xml:id="l.15">Nyilván meglátta minden nemzet,</l>
					<l xml:id="l.16"><app xml:id="app.5">
							<lem wit="#KK" xml:id="lem.5">Örvendgyen hát néki
								minden<corr>!</corr></lem>
							<rdg wit="#W1 #W2" xml:id="rdg.8">minden!</rdg>
							<rdg wit="#W3" xml:id="rdg.9">minden;</rdg>
						</app></l>
				</lg>
				<lg xml:id="lg.3">
					<fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
					<l xml:id="l.17">Örüllyetec és vigadgyatoc,</l>
					<l xml:id="l.18">Mondgyatoc szép énekeket,</l>
					<l xml:id="l.19">Cimbalmockal hangiczállyatoc,</l>
					<l xml:id="l.20"><app xml:id="app.6">
							<lem wit="#KK" xml:id="lem.6">Zönditsetec hegedüket<corr>!</corr></lem>
							<rdg wit="#W1 #W2" xml:id="rdg.10">hegedüket!</rdg>
							<rdg wit="#W3" xml:id="rdg.11">hegedüket;</rdg>
						</app></l>
					<l rend="level_1" xml:id="l.21">Az trombitákat fujátoc meg,</l>
					<l xml:id="l.22">Ez király elött zengjetec,</l>
					<l xml:id="l.23"><app xml:id="app.7">
							<lem wit="#KK" xml:id="lem.7">Czendüllyetec zudullyatoc meg</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.12">zudullyátok</rdg>
						</app></l>
					<l xml:id="l.24">Tengeren földön mindenec.</l>
				</lg>
				<lg xml:id="lg.4">
					<fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
					<l xml:id="l.25">AZ Ur elött az folyovizec</l>
					<l xml:id="l.26">Örvendezzenec mindnyájan,</l>
					<l xml:id="l.27">Az magas hegyec az Istennec</l>
					<l xml:id="l.28"><app xml:id="app.8">
							<lem wit="#KK" xml:id="lem.8">Tapsollyanac vig voltokban.</lem>
							<rdg wit="#W2" xml:id="rdg.13">Tapsollyánac</rdg>
						</app></l>
					<l rend="level_1" xml:id="l.29">Mert imé eljö törvént tenni,</l>
					<l xml:id="l.30">Megitél ez földön mindent,</l>
					<l xml:id="l.31">Mind ez világot jol rendeli,</l>
					<l xml:id="l.32">Az tiszta igasság szerént.</l>
				</lg>
			</div>
		</body>
