    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 163.</fw>
                <head type="title">SZÜZ MARIANAC ENEKE. LUC. I.</head>
                <head type="notation">Az 146. Soltarnac Franciai notájára.</head>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">MAgasztallya az én szivem,</l>
                    <l xml:id="l.2">Az Urat mind éltiglen,</l>
                    <l xml:id="l.3">Es örvendez az én lelkem,</l>
                    <l xml:id="l.4">Megtarto Istenemben,</l>
                    <l xml:id="l.5"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">Hirdetem kegy<corr>e</corr>sségét,</lem>
                            <rdg wit="#W1" xml:id="rdg.1">kegysségét</rdg>
                        </app></l>
                    <l xml:id="l.6">Es nagy jótéteményét.</l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.7">Mert kegyessen megtekinté,</l>
                    <l xml:id="l.8">Oe szolgálóléányát,</l>
                    <l xml:id="l.9">Ióvóltából meg nem veté,</l>
                    <l xml:id="l.10">Hiv alázatos voltát,</l>
                    <l xml:id="l.11">Mert imé minden nemzet,</l>
                    <l xml:id="l.12">Bódognac mond engemet.</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.13">Mert én velem nagy dolgakat</l>
                    <l xml:id="l.14">Tön az hatalmas Isten,</l>
                    <l xml:id="l.15">Midön egyigyü vóltamat</l>
                    <l xml:id="l.16">Felemelé kegyessen,</l>
                    <l xml:id="l.17">Szent azért az ö neve,</l>
                    <l xml:id="l.18">Aldando diczösége.</l>
                </lg>
                <lg xml:id="lg.4">
                    <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                    <l xml:id="l.19">Es az ö irgalma vagyon</l>
                    <l xml:id="l.20">Nemzetségröl nemzetre,</l>
                    <l xml:id="l.21">Oeröcké néz irgalmasson</l>
                    <l xml:id="l.22">Minden ötet félökre.</l>
                    <l xml:id="l.23">Hatalmas dolgot szerze,</l>
                    <l xml:id="l.24">Oe karjánac ereje.</l>
                </lg>
                <lg xml:id="lg.5">
                    <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                    <l xml:id="l.25">Az kevély gondolatúknac,</l>
                    <l xml:id="l.26">Ménten eszeket veszté,</l>
                    <l xml:id="l.27">Dihös szándékát azoknac</l>
                    <l xml:id="l.28">Az jokra nem ereszté,</l>
                    <l xml:id="l.29">Engemis nagy irgalma</l>
                    <l xml:id="l.30">Megtarta nagy hatalma.</l>
                </lg>
                <lg xml:id="lg.6">
                    <fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
                    <l xml:id="l.31">Az hatalmas gonoszokat</l>
                    <l xml:id="l.32">Oe székekböl levoná,</l>
                    <l xml:id="l.33">Es az alázatosokat</l>
                    <l xml:id="l.34">Hiven felmagasztalá.</l>
                    <l xml:id="l.35">Mert szeret egyigyöket,</l>
                    <l xml:id="l.36">Es gyülöl kevélyöket.</l>
                </lg>
                <lg xml:id="lg.7">
                    <fw corresp="Szenci Molnár Albert" type="lg">7 </fw>
                    <l xml:id="l.37">Az nyomorult éhezöket</l>
                    <l xml:id="l.38">Minden jockal bétölté,</l>
                    <l xml:id="l.39">Az gazdagsággal tölteket</l>
                    <l xml:id="l.40">Üressen elereszté,</l>
                    <l xml:id="l.41">Mert ö töle mindenec</l>
                    <l xml:id="l.42">Bölczen vezéreltetnec.</l>
                </lg>
                <lg xml:id="lg.8">
                    <fw corresp="Szenci Molnár Albert" type="lg">8 </fw>
                    <l xml:id="l.43">Izraelt az ö szolgáját</l>
                    <l xml:id="l.44">Hiven hozzá fogadá</l>
                    <l xml:id="l.45">Emlitvén irgalmasságát,</l>
                    <l xml:id="l.46">Népét meglátogatá.</l>
                    <l xml:id="l.47">Rájoc nyuytván kegyelmét</l>
                    <l xml:id="l.48">Bétölté igéretét.</l>
                </lg>
                <lg xml:id="lg.9">
                    <fw corresp="Szenci Molnár Albert" type="lg">9 </fw>
                    <l xml:id="l.49">Az miképpen szolott vala,</l>
                    <l xml:id="l.50">Mi elsö atyáinknac,</l>
                    <l xml:id="l.51">Abrahamnac mit megmonda</l>
                    <l xml:id="l.52">Es ö maradékinac,</l>
                    <l xml:id="l.53">Im mind megtellyesité,</l>
                    <l xml:id="l.54">Kiért aldgyuc öröcké.</l>
                </lg>
            </div>
