                         <text>
                                                  <body>
                                                  <div type="poem" xml:id="d.1" xml:lang="hu">
                                                  <fw type="originalNum">[RMKT XVII/6:] 123.</fw>
                                                  <head type="title">CXXIII. SOLTAR. T. B. </head>
                                                  <head type="sub">Könyörgése az Izrael népenec az
                                                  megszabaditasert.</head>
                                                  <pb facs="293"/>
                                                  <lg xml:id="lg.1">
                                                  <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                                                  <l xml:id="l.1">TE hozzád szemeimet Ur Isten</l>
                                                  <l xml:id="l.2">Emelem az égben,</l>
                                                  <l xml:id="l.3"><app xml:id="app.1">
                                                  <lem wit="#KK" xml:id="lem.1">Mint
                                                  szolg<corr>á</corr>nac szemei nyitva vadnac,</lem>
                                                  <rdg wit="#W1 #W2" xml:id="rdg.1">szolgánac</rdg>
                                                  <rdg wit="#W3" xml:id="rdg.2">szolganac</rdg>
                                                  </app></l>
                                                  <l xml:id="l.4">Es az Urra vigyáznac.</l>
                                                  <l xml:id="l.5">Mint szólgálo leány ö
                                                  aszszonyára</l>
                                                  <l xml:id="l.6"><app xml:id="app.2">
                                                  <lem wit="#KK" xml:id="lem.2">Tekint minden
                                                  or<corr>á</corr>ba:</lem>
                                                  <rdg wit="#W1" xml:id="rdg.3">orába</rdg>
                                                  <rdg wit="#W2 #W3" xml:id="rdg.4">oraba</rdg>
                                                  </app></l>
                                                  <l xml:id="l.7">Ugy néznec szemeinc az Istenre</l>
                                                  <l xml:id="l.8">Es ö kegyelmére.</l>
                                                  </lg>
                                                  <lg xml:id="lg.2">
                                                  <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                                                  <l xml:id="l.9"><app xml:id="app.3">
                                                  <lem wit="#KK" xml:id="lem.3"
                                                  >K<corr>e</corr>gyelmezz Uram kegielmezz
                                                  nékünc,</lem>
                                                  <rdg wit="#W1" xml:id="rdg.5">Kégjelmez</rdg>
                                                  <rdg wit="#W2 #W3" xml:id="rdg.6">Kégyelmezz</rdg>
                                                  </app></l>
                                                  <l xml:id="l.10">Mert nincz böczületünc,</l>
                                                  <l xml:id="l.11">Minden olly szertelenül gyaláz
                                                  minket,</l>
                                                  <l xml:id="l.12">Kinél föllyeb nem lehet:</l>
                                                  <l xml:id="l.13">Kevély népec minket szörnyen
                                                  nevetnec,</l>
                                                  <l xml:id="l.14">Raytunc czufságot üznec,</l>
                                                  <l xml:id="l.15">Negédes szóckal ugy
                                                  nevettetünc,</l>
                                                  <l xml:id="l.16">Kikvel eltölt lelkünc.</l>
                                                  </lg>
                                                  </div>
