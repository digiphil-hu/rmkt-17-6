    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 149.</fw>
                <head type="title">CXLIX. SOLTAR. T. B.</head>
                <head type="sub">Intés az Istennec diczéretire.</head>
                <pb facs="333"/>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">AZ Urnac nó énekellyetec</l>
                    <l xml:id="l.2"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">Uy ének<corr>e</corr>t
                                szentfölségenec,</lem>
                            <rdg wit="#W1" xml:id="rdg.1">éneket</rdg>
                            <rdg wit="#W2 #W3" xml:id="rdg.2">énekét</rdg>
                            <rdg wit="#W1" xml:id="rdg.3">fölséginec</rdg>
                        </app></l>
                    <l xml:id="l.3"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Az
                                szentecgyülek<corr>e</corr>zetiben</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.4">gyülekezetiben</rdg>
                            <rdg wit="#W3" xml:id="rdg.5">gyülekézetiben</rdg>
                        </app></l>
                    <l xml:id="l.4">Diczérje ötet minden,</l>
                    <l xml:id="l.5">Az Izrael örvendezzen,</l>
                    <l xml:id="l.6">Ö teremtö Istenében,</l>
                    <l xml:id="l.7">Királlyoknac örüllyenec</l>
                    <l xml:id="l.8">Az Sionbéliec.</l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.9"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Az Ur nevét sippal és dobbal,</lem>
                            <rdg wit="#W1" xml:id="rdg.6">nevit</rdg>
                            <rdg wit="#W1 #W2" xml:id="rdg.7">sipval es dobval</rdg>
                        </app></l>
                    <l xml:id="l.10"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Diczérjétec lanttal kobozzal,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.8">lantval</rdg>
                        </app></l>
                    <l xml:id="l.11">Légyen hegedöknec zengése</l>
                    <l xml:id="l.12">Neve diczéretire.</l>
                    <l xml:id="l.13">Mert az Isten az ö népit</l>
                    <l xml:id="l.14">Igen szereti hiveit.</l>
                    <l xml:id="l.15"><app xml:id="app.5">
                            <lem wit="#KK" xml:id="lem.5">Az szegényeket segeti,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.9">segéti</rdg>
                        </app></l>
                    <l xml:id="l.16">Sok jockal szereti.</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.17">Szent hivei az Ur Istennec</l>
                    <l xml:id="l.18">Ekes gyönyörüséget nyernec,</l>
                    <l xml:id="l.19">Örülnec ö háló házokban</l>
                    <l xml:id="l.20">Az Urat magaztalvan:</l>
                    <l xml:id="l.21"><app xml:id="app.6">
                            <lem wit="#KK" xml:id="lem.6">Diczéretét az Istennec</lem>
                            <rdg wit="#W1" xml:id="rdg.10">Dicziretét</rdg>
                        </app></l>
                    <l xml:id="l.22">Az ö szájokban visellyéc,</l>
                    <l xml:id="l.23"><app xml:id="app.7">
                            <lem wit="#KK" xml:id="lem.7">Es kétélö fegyverl<corr>e</corr>szen</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.11">leszen</rdg>
                            <rdg wit="#W3" xml:id="rdg.12">leeszen</rdg>
                        </app></l>
                    <l xml:id="l.24">Nékiec kezekben.</l>
                </lg>
                <lg xml:id="lg.4">
                    <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                    <l xml:id="l.25"><app xml:id="app.8">
                            <lem wit="#KK" xml:id="lem.8">Mellyel az kevély ellenségen</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.13">kevél</rdg>
                        </app></l>
                    <l xml:id="l.26"><app xml:id="app.9">
                            <lem wit="#KK" xml:id="lem.9">Boszszut állanac nagy mérészen,</lem>
                            <rdg wit="#W1" xml:id="rdg.14">mérészszen</rdg>
                        </app></l>
                    <l xml:id="l.27">Es az népec megbüntettetnec,</l>
                    <l xml:id="l.28">Mélto jutalmat vesznec:</l>
                    <l xml:id="l.29">Erös királlyokat nékic,</l>
                    <l xml:id="l.30">Nagy lántzokkal megkötözic,</l>
                    <l xml:id="l.31">Es minden ö fejedelmec</l>
                    <l xml:id="l.32"><app xml:id="app.10">
                            <lem wit="#KK" xml:id="lem.10">V<corr>a</corr>s békókban ésnec.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.15">Vas</rdg>
                            <rdg wit="#W3" xml:id="rdg.16">Vás</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.5">
                    <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                    <l xml:id="l.33">Hogy törvénnyel itéltessenec,</l>
                    <l xml:id="l.34">Melly megvagyon irván ellenec:</l>
                    <l xml:id="l.35">Mellyekböl az istenes hivec</l>
                    <l xml:id="l.36">Diczéretet nyernec.</l>
                </lg>
            </div>
