	<text>
		<body>
			<div type="poem" xml:id="d.1" xml:lang="hu">
				<fw type="originalNum">[RMKT XVII/6:] 76.</fw>
				<head type="title">LXXVI. SOLTAR. T. B. 30.139.</head>
				<head type="sub"><app xml:id="app.1">
						<lem wit="#KK" xml:id="lem.1">Háláadása az Juda népénec az Aszyriusoc ellen
							valo gyözödelemért.</lem>
						<rdg wit="#W1" xml:id="rdg.1">Assyriusok</rdg>
						<rdg wit="#W2" xml:id="rdg.2">Assyriusoc […] gyözédelemért</rdg>
					</app></head>
				<head type="notation">Az XXX. Soltar notajára.</head>
				<lg xml:id="lg.1">
					<fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
					<l xml:id="l.1">ESmeretes az Ur Isten</l>
					<l xml:id="l.2"><app xml:id="app.2">
							<lem wit="#KK" xml:id="lem.2">Az Judában, es szent neve</lem>
							<rdg wit="#W2" xml:id="rdg.3">és</rdg>
						</app></l>
					<l xml:id="l.3">Az Izraelnec földében</l>
					<l xml:id="l.4"><app xml:id="app.3">
							<lem wit="#KK" xml:id="lem.3">Meszsze terjett diczösége,</lem>
							<rdg wit="#W1" xml:id="rdg.4">düczösége</rdg>
						</app></l>
					<l xml:id="l.5"><app xml:id="app.4">
							<lem wit="#KK" xml:id="lem.4">Az Salemben szép ö
								s<corr>á</corr>tora</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.5">sátora</rdg>
							<rdg wit="#W3" xml:id="rdg.6">satora</rdg>
						</app></l>
					<l xml:id="l.6">Az Sionon lako hayléka.</l>
				</lg>
				<lg xml:id="lg.2">
					<fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
					<l xml:id="l.7">Ott megláttya minden ember,</l>
					<l xml:id="l.8">Hogy Isteni hatalmával</l>
					<l xml:id="l.9">Megtöric Iy, pais, fegyver,</l>
					<l xml:id="l.10">Hadat megszállit azonnal,</l>
					<l xml:id="l.11"><app xml:id="app.5">
							<lem wit="#KK" xml:id="lem.5">Nagyob felségednec ereje,</lem>
							<rdg wit="#W1" xml:id="rdg.7">fölségednec</rdg>
						</app></l>
					<l xml:id="l.12">Hogynem az kóborlóc soc hegye.</l>
				</lg>
				<lg xml:id="lg.3">
					<fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
					<l xml:id="l.13">Az ellenség eldulatéc,</l>
					<l xml:id="l.14">Reájoc mély álom eséc,</l>
					<l xml:id="l.15"><app xml:id="app.6">
							<lem wit="#KK" xml:id="lem.6">Kiket vitézeknec véltec</lem>
							<rdg wit="#W2" xml:id="rdg.8">Kikét</rdg>
						</app></l>
					<l xml:id="l.16">Tudatlan lön az ö kezec.</l>
					<l xml:id="l.17">Mihelyen feddésed hallatic,</l>
					<l xml:id="l.18"><app xml:id="app.7">
							<lem wit="#KK" xml:id="lem.7">Mind szeker s’ mind ló
									elalus<corr>z</corr>ic.</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.9">elaluszic</rdg>
							<rdg wit="#W3" xml:id="rdg.10">elaluszzic</rdg>
						</app></l>
				</lg>
				<lg xml:id="lg.4">
					<fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
					<l xml:id="l.19">Ki álhatna meg elötted,</l>
					<l xml:id="l.20">Az ki illy rettenetes vagy,</l>
					<l xml:id="l.21">Midön haragod fölgerjed?</l>
					<l xml:id="l.22">Menybéli sentenciád nagy,</l>
					<l xml:id="l.23"><app xml:id="app.8">
							<lem wit="#KK" xml:id="lem.8">Mellyet hallatsz az
								emb<corr>e</corr>reckel,</lem>
							<rdg wit="#W1" xml:id="rdg.11">emberekkel</rdg>
							<rdg wit="#W2" xml:id="rdg.12">embereckel</rdg>
							<rdg wit="#W3" xml:id="rdg.13">embéreckel</rdg>
						</app></l>
					<l xml:id="l.24">Az föld megrémül czendességgel.</l>
				</lg>
				<lg xml:id="lg.5">
					<fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
					<l xml:id="l.25"><app xml:id="app.9">
							<lem wit="#KK" xml:id="lem.9">Midön fölkél az Ur Isten,</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.14">fölkéll</rdg>
						</app></l>
					<l xml:id="l.26">Tartván kemény itéletit,</l>
					<l xml:id="l.27"><app xml:id="app.10">
							<lem wit="#KK" xml:id="lem.10">Hogy az sz<corr>egé</corr>nyeket
								itten</lem>
							<rdg wit="#W1" xml:id="rdg.15">szegénieket</rdg>
							<rdg wit="#W2" xml:id="rdg.16">szegényeket</rdg>
							<rdg wit="#W3" xml:id="rdg.17">szégenyeket</rdg>
						</app></l>
					<l xml:id="l.28">Ez földön megtartsa népit,</l>
					<l xml:id="l.29">Diczéretedre fordul néked,</l>
					<l xml:id="l.30">Ha az nép haragszic ellened.</l>
				</lg>
				<lg xml:id="lg.6">
					<fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
					<l xml:id="l.31">Még egyszer nyilván elveszted</l>
					<l xml:id="l.32">Ez maradéc dúlótfúlót,</l>
					<l xml:id="l.33">Jer diczérjüc Istenünket,</l>
					<l xml:id="l.34"><app xml:id="app.11">
							<lem wit="#KK" xml:id="lem.11">Megálvan fogad<corr>á</corr>stokot,</lem>
							<rdg wit="#W1" xml:id="rdg.18">fogadástokot</rdg>
							<rdg wit="#W2 #W3" xml:id="rdg.19">fogadastokot</rdg>
						</app></l>
					<l xml:id="l.35">Kic mindenkor vattoc ö véle</l>
					<l xml:id="l.36">Es el nem távoztoc ö töle.</l>
				</lg>
				<lg xml:id="lg.7">
					<fw corresp="Szenci Molnár Albert" type="lg">7 </fw>
					<l xml:id="l.37"><app xml:id="app.12">
							<lem wit="#KK" xml:id="lem.12">Adgyatoc szép
								ay<corr>á</corr>ndékot</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.20">ayándékot</rdg>
							<rdg wit="#W3" xml:id="rdg.21">ayandékot</rdg>
						</app></l>
					<l xml:id="l.38">Ez rettenetes Istennec,</l>
					<l xml:id="l.39">Ki megtöri hatalmakot</l>
					<l xml:id="l.40">Az gonosz fejedelmeknec,</l>
					<l xml:id="l.41">Az nagy királyoc itt ez földen</l>
					<l xml:id="l.42">Elötte lesznec rettegésben.</l>
				</lg>
			</div>
		</body>
	</text>
