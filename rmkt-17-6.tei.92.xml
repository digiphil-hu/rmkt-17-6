                         <text>
                                                  <body>
                                                  <div type="poem" xml:id="d.1" xml:lang="hu">
                                                  <fw type="originalNum">[RMKT XVII/6:] 92.</fw>
                                                  <head type="title"><app xml:id="app.1">
                                                  <lem wit="#KK" xml:id="lem.1">XCII. SOLTAR.
                                                  <corr>T.</corr> B.</lem>
                                                  <rdg wit="#W1 #W2" xml:id="rdg.1">T. B.</rdg>
                                                  <rdg wit="#W3" xml:id="rdg.2">S. B.</rdg>
                                                  </app></head>
                                                  <head type="sub"><app xml:id="app.2">
                                                  <lem wit="#KK" xml:id="lem.2">Háláadás az Istennec
                                                  gondviseléséröl.</lem>
                                                  <rdg wit="#W1" xml:id="rdg.3"
                                                  >gondviselésiröl</rdg>
                                                  </app></head>
                                                  <pb facs="217"> </pb>
                                                  <lg xml:id="lg.1">
                                                  <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                                                  <l xml:id="l.1"><app xml:id="app.3">
                                                  <lem wit="#KK" xml:id="lem.3">EK<corr>e</corr>s
                                                  dolog diczérni</lem>
                                                  <rdg wit="#W1" xml:id="rdg.4">EKes</rdg>
                                                  <rdg wit="#W2 #W3" xml:id="rdg.5">EKés</rdg>
                                                  </app></l>
                                                  <l xml:id="l.2"><app xml:id="app.4">
                                                  <lem wit="#KK" xml:id="lem.4">Uram
                                                  föls<corr>ége</corr>det,</lem>
                                                  <rdg wit="#W1 #W2 #W3" xml:id="rdg.6"
                                                  >fölsegédet</rdg>
                                                  </app></l>
                                                  <l xml:id="l.3">Es az te nevedet</l>
                                                  <l xml:id="l.4">Enekvel magaztalni,</l>
                                                  <l rend="level_1" xml:id="l.5">Hogy ember áldgya
                                                  reggel</l>
                                                  <l xml:id="l.6">Te nagy jovoltodat</l>
                                                  <l xml:id="l.7">Es igasságodat</l>
                                                  <l xml:id="l.8">Diczérje minden éyjel.</l>
                                                  </lg>
                                                  <lg xml:id="lg.2">
                                                  <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                                                  <l xml:id="l.9">Lantban és hegedöben,</l>
                                                  <l xml:id="l.10">Es szép cimbalmokban,</l>
                                                  <l xml:id="l.11">Hangos citerákban,</l>
                                                  <l xml:id="l.12">Diczértessél zengésben.</l>
                                                  <l rend="level_1" xml:id="l.13">Dolgaidon
                                                  örvendec</l>
                                                  <l xml:id="l.14">Hatalmadat látván,</l>
                                                  <l xml:id="l.15">Kezednec czudáján</l>
                                                  <l xml:id="l.16">Örömömben éneklec.</l>
                                                  </lg>
                                                  <lg xml:id="lg.3">
                                                  <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                                                  <l xml:id="l.17">Soc és nagy czudálatos</l>
                                                  <l xml:id="l.18">Te czelekedeted,</l>
                                                  <l xml:id="l.19">Mélség bölczeséged,</l>
                                                  <l xml:id="l.20">Beszéded drágalatos:</l>
                                                  <l rend="level_1" xml:id="l.21">Ez dolgot az
                                                  esztelen</l>
                                                  <l xml:id="l.22">Egy szálné sem érti,</l>
                                                  <l xml:id="l.23">Es meg sem tekinti</l>
                                                  <l xml:id="l.24">Hogy ez miképpen legyen:</l>
                                                  </lg>
                                                  <lg xml:id="lg.4">
                                                  <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                                                  <l xml:id="l.25">Hogy az gonoszoc nönec</l>
                                                  <l xml:id="l.26">Mint az fü az mezön,</l>
                                                  <l xml:id="l.27">Virágoznac szépön</l>
                                                  <l xml:id="l.28">Az soc istentelenec.</l>
                                                  <l rend="level_1" xml:id="l.29">Hogy öröcké
                                                  essenec</l>
                                                  <l xml:id="l.30">Az veszedelemben,</l>
                                                  <l xml:id="l.31">Te vagy öröc Isten</l>
                                                  <l xml:id="l.32">Fölötte mindeneknec.</l>
                                                  </lg>
                                                  <lg xml:id="lg.5">
                                                  <metamark function="pause"/>
                                                  <fw corresp="Szenci Molnár Albert" type="lg"><hi
                                                  rend="super">*</hi><hi rend="sub">*</hi><hi
                                                  rend="super">*</hi> 5 </fw>
                                                  <l xml:id="l.33">Mert im az hitetlenec</l>
                                                  <l xml:id="l.34">Az te ellenségid,</l>
                                                  <l xml:id="l.35">Minden gyülölöid</l>
                                                  <l xml:id="l.36">Szörnyüképpen elvesznec.</l>
                                                  <l rend="level_1" xml:id="l.37"><app
                                                  xml:id="app.5">
                                                  <lem wit="#KK" xml:id="lem.5">Szarvamat
                                                  fölemeled</lem>
                                                  <rdg wit="#W2" xml:id="rdg.7">Szárvamat</rdg>
                                                  </app></l>
                                                  <l xml:id="l.38">Mint az Egyszarvúnac,</l>
                                                  <l xml:id="l.39">Hogy én vigadhassac</l>
                                                  <l xml:id="l.40">En Istenem te benned.</l>
                                                  </lg>
                                                  <lg xml:id="lg.6">
                                                  <fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
                                                  <l xml:id="l.41">Megkenic én fejemet</l>
                                                  <l xml:id="l.42">Zöldellö olayval,</l>
                                                  <l xml:id="l.43">Hogy diadalomval</l>
                                                  <l xml:id="l.44">Lássam ellenségimet.</l>
                                                  <l rend="level_1" xml:id="l.45">Végre én
                                                  füleimmel,</l>
                                                  <l xml:id="l.46">En ellenségimröl</l>
                                                  <l xml:id="l.47">Es gyülölöimröl</l>
                                                  <l xml:id="l.48">Uy hirt halloc örömmel.</l>
                                                  </lg>
                                                  <lg xml:id="lg.7">
                                                  <fw corresp="Szenci Molnár Albert" type="lg">7 </fw>
                                                  <l xml:id="l.49">Virágoznac az hivec,</l>
                                                  <l xml:id="l.50">Mint az szép pálmafác,</l>
                                                  <l xml:id="l.51">Es mint az cedrus fác,</l>
                                                  <l xml:id="l.52">Kic az Libanon nönec.</l>
                                                  <l rend="level_1" xml:id="l.53">Es az Ur
                                                  haylékában</l>
                                                  <l xml:id="l.54">Ezec plántáltatnac</l>
                                                  <l xml:id="l.55">Szépen virágoznac</l>
                                                  <l xml:id="l.56">Az Isten tornátzában.</l>
                                                  </lg>
                                                  <lg xml:id="lg.8">
                                                  <fw corresp="Szenci Molnár Albert" type="lg">8 </fw>
                                                  <l xml:id="l.57">Hogy ha megöszülnekis,</l>
                                                  <l xml:id="l.58"><app xml:id="app.6">
                                                  <lem wit="#KK" xml:id="lem.6">De
                                                  mindazonáltal,</lem>
                                                  <rdg wit="#W1" xml:id="rdg.8">mind azon
                                                  által</rdg>
                                                  </app></l>
                                                  <l xml:id="l.59">Nagy szaporasággal,</l>
                                                  <l xml:id="l.60">Gyümölczöt hoznac mégis.</l>
                                                  <l rend="level_1" xml:id="l.61">Hogy igassagát
                                                  hiven</l>
                                                  <l xml:id="l.62">Mindenütt hirdesséc</l>
                                                  <l xml:id="l.63">Az én Istenemnec,</l>
                                                  <l xml:id="l.64">Kiben hamisság ninczen.</l>
                                                  </lg>
                                                  </div>
                                                  </body>
