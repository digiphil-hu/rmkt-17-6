    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 80.</fw>
                <head type="title">LXXX. SOLTAR. T. B.</head>
                <head type="sub">Könyörgése az Sidoknac az megszabadulásért.</head>
                <pb facs="193"> </pb>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">HAlgasd meg Izrael pasztora,</lem>
                            <rdg wit="#W1" xml:id="rdg.1">pastora</rdg>
                        </app></l>
                    <l xml:id="l.2">Es Josephnec vezérlö Ura,</l>
                    <l xml:id="l.3">Kit örizsz mint egy juh nyáyjat,</l>
                    <l xml:id="l.4"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Fordits hozzánc szent ortzádat,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.2">hozzánc</rdg>
                            <rdg wit="#W3" xml:id="rdg.3">hózzánc</rdg>
                        </app></l>
                    <l xml:id="l.5">Ki ülsz az Cherubimokon,</l>
                    <l xml:id="l.6">Jelennyél meg világosson.</l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.7">Mutasd meg Uram nagy erödöt,</l>
                    <l xml:id="l.8"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">L<corr>á</corr>ttasd meg az Ephraim
                                elöt,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.4">Láttasd</rdg>
                            <rdg wit="#W3" xml:id="rdg.5">Lattasd</rdg>
                        </app></l>
                    <l xml:id="l.9"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Mutasd meg Benjaminnakis,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.6">Bénjaminnakis</rdg>
                        </app></l>
                    <l xml:id="l.10">Igy tégy az Manassénakis,</l>
                    <l xml:id="l.11">Tarts meg minket tellyességgel,</l>
                    <l xml:id="l.12">Nagy inségünkben ne hagy el.</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.13"><app xml:id="app.5">
                            <lem wit="#KK" xml:id="lem.5">Ur Isten t<corr>é</corr>rj hozzánc
                                ismetlen,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.7">térj</rdg>
                            <rdg wit="#W3" xml:id="rdg.8">terj</rdg>
                        </app></l>
                    <l xml:id="l.14">Örizz meg minden gonosz ellen,</l>
                    <l xml:id="l.15">Világosságodat nyuytsad,</l>
                    <l xml:id="l.16">Irgalmadat ránc forditsad,</l>
                    <l xml:id="l.17">Térits hozzánc szent ortzádat,</l>
                    <l xml:id="l.18">Es semmi nékünc nem árthat.</l>
                </lg>
                <lg xml:id="lg.4">
                    <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                    <l xml:id="l.19">Oh Ur Isten vallyon mig tartod</l>
                    <l xml:id="l.20">Mi raytunc az te nagy haragod?</l>
                    <l xml:id="l.21">Ha adod meg kérésünket?</l>
                    <l xml:id="l.22"><app xml:id="app.6">
                            <lem wit="#KK" xml:id="lem.6">Könyhullat<corr>á</corr>st kenyér
                                hellyet</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.9">Könyhullatást</rdg>
                            <rdg wit="#W3" xml:id="rdg.10">Könyhullatast</rdg>
                            <rdg wit="#W1 #W2" xml:id="rdg.11">helyet</rdg>
                        </app></l>
                    <l xml:id="l.23">Adál nékünc, és itatál</l>
                    <l xml:id="l.24">Minket nagy könyhullatással.</l>
                </lg>
                <lg xml:id="lg.5">
                    <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                    <l xml:id="l.25"><app xml:id="app.7">
                            <lem wit="#KK" xml:id="lem.7">Te minket feddöd<corr>é</corr>sben
                                eytél,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.12">feddödésben</rdg>
                            <rdg wit="#W3" xml:id="rdg.13">feddödesben</rdg>
                        </app></l>
                    <l xml:id="l.26"><app xml:id="app.8">
                            <lem wit="#KK" xml:id="lem.8">Szomszédin<corr>c</corr>kal nem
                                böczültetél,</lem>
                            <rdg wit="#W1" xml:id="rdg.14">Szomszeddinkkal</rdg>
                            <rdg wit="#W2" xml:id="rdg.15">Szomszeddinckal</rdg>
                            <rdg wit="#W3" xml:id="rdg.16">Szomszéd inkal</rdg>
                        </app></l>
                    <l xml:id="l.27">Az ellenségnec lönc czufja,</l>
                    <l xml:id="l.28"><app xml:id="app.9">
                            <lem wit="#KK" xml:id="lem.9">Tarts meg sereg<corr>e</corr>knec
                                Ura,</lem>
                            <rdg wit="#W1 #W2 #W3" xml:id="rdg.17">seregéknec</rdg>
                        </app></l>
                    <l xml:id="l.29">Világosits szined raytunc,</l>
                    <l xml:id="l.30"><app xml:id="app.10">
                            <lem wit="#KK" xml:id="lem.10">Es nem lesz semmi
                                bánt<corr>á</corr>sunc.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.18">bántásunc</rdg>
                            <rdg wit="#W3" xml:id="rdg.19">bántasunc</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.6">
                    <metamark function="pause"/>
                    <fw corresp="Szenci Molnár Albert" type="lg"><hi rend="super">*</hi><hi
                            rend="sub">*</hi><hi rend="super">*</hi> 6 </fw>
                    <l xml:id="l.31">Az szölötövet Egyptombol</l>
                    <l xml:id="l.32">Elhozád az sovány országbol,</l>
                    <l xml:id="l.33"><app xml:id="app.11">
                            <lem wit="#KK" xml:id="lem.11">Es az jo földbe plántálád,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.20">földben</rdg>
                        </app></l>
                    <l xml:id="l.34">Honnan az népet kihaytád,</l>
                    <l xml:id="l.35"><app xml:id="app.12">
                            <lem wit="#KK" xml:id="lem.12">Es tágas helyen nagy meszsze</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.21">Ez</rdg>
                        </app></l>
                    <l xml:id="l.36">Ö gyökere kiterjede.</l>
                </lg>
                <lg xml:id="lg.7">
                    <fw corresp="Szenci Molnár Albert" type="lg">7 </fw>
                    <l xml:id="l.37"><app xml:id="app.13">
                            <lem wit="#KK" xml:id="lem.13">Ö széles árnyékja
                                k<corr>i</corr>terjet,</lem>
                            <rdg wit="#W1" xml:id="rdg.22">kiterjet</rdg>
                            <rdg wit="#W2 #W3" xml:id="rdg.23">kirterjet</rdg>
                        </app></l>
                    <l xml:id="l.38">Béfedte az magas hegyeket,</l>
                    <l xml:id="l.39">Az ö veszszei magassac,</l>
                    <l xml:id="l.40">Fölnöttec mint az cedrus fác,</l>
                    <l xml:id="l.41">Agait mind az nagy vizig</l>
                    <l xml:id="l.42">Kiterjeszté az tengerig.</l>
                </lg>
                <lg xml:id="lg.8">
                    <fw corresp="Szenci Molnár Albert" type="lg">8 </fw>
                    <l xml:id="l.43"><app xml:id="app.14">
                            <lem wit="#KK" xml:id="lem.14">Miért tördeléd el gyöpüjét?</lem>
                            <rdg wit="#W2" xml:id="rdg.24">türdeléd</rdg>
                        </app></l>
                    <l xml:id="l.44">Hogy eresztél belé más népet,</l>
                    <l xml:id="l.45">Ki szöleit leszaggatá?</l>
                    <l xml:id="l.46">Az erdei kan kiturá,</l>
                    <l xml:id="l.47">Az vadac kinyövéc szörnyen,</l>
                    <l xml:id="l.48">Hogy engeded ezt Ur Isten?</l>
                </lg>
                <lg xml:id="lg.9">
                    <fw corresp="Szenci Molnár Albert" type="lg">9 </fw>
                    <l xml:id="l.49">Uram ismétlen térj mihozzánc,</l>
                    <l xml:id="l.50">Mennyekböl szemed fordits reánc,</l>
                    <l xml:id="l.51">Es látogasd meg az szölöt,</l>
                    <l xml:id="l.52"><app xml:id="app.15">
                            <lem wit="#KK" xml:id="lem.15">Mellyet jobkezed
                                ült<corr>e</corr>töt,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.25">ültetöt</rdg>
                            <rdg wit="#W3" xml:id="rdg.26">ültétöt</rdg>
                        </app></l>
                    <l xml:id="l.53"><app xml:id="app.16">
                            <lem wit="#KK" xml:id="lem.16">Nézd meg ez czemétét
                                v<corr>é</corr>gre,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.27">végre</rdg>
                            <rdg wit="#W3" xml:id="rdg.28">vegre</rdg>
                        </app></l>
                    <l xml:id="l.54"><app xml:id="app.17">
                            <lem wit="#KK" xml:id="lem.17">Kit szerzél
                                diczös<corr>é</corr>gedre.</lem>
                            <rdg wit="#W1" xml:id="rdg.29">düczöségedre</rdg>
                            <rdg wit="#W2" xml:id="rdg.30">diczöségedre</rdg>
                            <rdg wit="#W3" xml:id="rdg.31">diczösegedre</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.10">
                    <fw corresp="Szenci Molnár Albert" type="lg">10 </fw>
                    <l xml:id="l.55">Tüzzel megint megégettetic,</l>
                    <l xml:id="l.56">Tellyességgel elpusztitatic</l>
                    <l xml:id="l.57">Haragodnac nagy tüzében:</l>
                    <l xml:id="l.58">Nyuyts kezedet, oh Ur Isten</l>
                    <l xml:id="l.59">Az emberre, kit kezeddel</l>
                    <l xml:id="l.60">Magadnac erösitöttél.</l>
                </lg>
                <lg xml:id="lg.11">
                    <fw corresp="Szenci Molnár Albert" type="lg">11 </fw>
                    <l xml:id="l.61">Es mi el nem térünc te töled,</l>
                    <l xml:id="l.62">Czac életünket erösitsed,</l>
                    <l xml:id="l.63">Es diczérjüc szent nevedet,</l>
                    <l xml:id="l.64">Ur Isten vigaztally minket,</l>
                    <l xml:id="l.65">Világosits szined raytunc,</l>
                    <l xml:id="l.66">Es mi nyilván megtartatunc.</l>
                </lg>
            </div>
        </body>
