	<text>
		<body>
			<div type="poem" xml:id="d.1" xml:lang="hu">
				<fw type="originalNum">[RMKT XVII/6:] 111.</fw>
				<head type="title">CXI. SOLTAR. T. B. 24.62.95.</head>
				<head type="sub">Háláadása az Izrael népénec.</head>
				<head type="notation">Az XXIV. Soltar notajára.</head>
				<lg xml:id="lg.1">
					<fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
					<l xml:id="l.1">HAlát adoc Uram néked,</l>
					<l xml:id="l.2">Tellyes szivböl áldlac téged,</l>
					<l xml:id="l.3">Az hiveknec ö seregiben</l>
					<l xml:id="l.4"><app xml:id="app.1">
							<lem wit="#KK" xml:id="lem.1">Megvallom diczöségedet,</lem>
							<rdg wit="#W1" xml:id="rdg.1">düczösegedet</rdg>
						</app></l>
					<l xml:id="l.5">Hirdetem diczéretedet</l>
					<l xml:id="l.6">Eletemnec minden rendiben.</l>
				</lg>
				<lg xml:id="lg.2">
					<fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
					<l xml:id="l.7">Nagyoc az Urnac czudai,</l>
					<l xml:id="l.8"><app xml:id="app.2">
							<lem wit="#KK" xml:id="lem.2">Es az ki azt megtekénti</lem>
							<rdg wit="#W2" xml:id="rdg.2">megtékenti</rdg>
						</app></l>
					<l xml:id="l.9">Örvendez annac az ö szive,</l>
					<l xml:id="l.10"><app xml:id="app.3">
							<lem wit="#KK" xml:id="lem.3">Az ö diczö méltosága</lem>
							<rdg wit="#W1" xml:id="rdg.3">düczö</rdg>
						</app></l>
					<l xml:id="l.11">Es az ö szent igassága</l>
					<l xml:id="l.12">Megmarad öröckön öröcké.</l>
				</lg>
				<lg xml:id="lg.3">
					<fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
					<l xml:id="l.13">Czuda dolgait az Isten</l>
					<l xml:id="l.14">Szerzette emlekezetben,</l>
					<l xml:id="l.15"><app xml:id="app.4">
							<lem wit="#KK" xml:id="lem.4">Kegyes és irgalmas kedvéböl,</lem>
							<rdg wit="#W1" xml:id="rdg.4">kedviböl</rdg>
						</app></l>
					<l xml:id="l.16">Etelt ád azoknac böven,</l>
					<l xml:id="l.17">Az kic ötet félic hiven,</l>
					<l xml:id="l.18"><app xml:id="app.5">
							<lem wit="#KK" xml:id="lem.5">Megemlekezic kötéséröl.</lem>
							<rdg wit="#W1" xml:id="rdg.5">kötésiröl</rdg>
						</app></l>
				</lg>
				<lg xml:id="lg.4">
					<fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
					<l xml:id="l.19">Népével nagy czudákat tet,</l>
					<l xml:id="l.20">Hogy az poganyoc örökét,</l>
					<l xml:id="l.21"><app xml:id="app.6">
							<lem wit="#KK" xml:id="lem.6">Kezekben adá ö nékiec:</lem>
							<rdg wit="#W1" xml:id="rdg.6">Oe nekiek kezekben ada:</rdg>
						</app></l>
					<l xml:id="l.22"><app xml:id="app.7">
							<lem wit="#KK" xml:id="lem.7">Merö hivség és tisztaság</lem>
							<rdg wit="#W1" xml:id="rdg.7">hüség</rdg>
						</app></l>
					<l xml:id="l.23">Es álhatatos igasság</l>
					<l xml:id="l.24">Minden dolga az ö kezénec.</l>
				</lg>
				<lg xml:id="lg.5">
					<fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
					<l xml:id="l.25">Minden ö paranczolattya</l>
					<l xml:id="l.26">Igaz minden ö hagyása,</l>
					<l xml:id="l.27"><app xml:id="app.8">
							<lem wit="#KK" xml:id="lem.8">Kiben semmi v<corr>á</corr>ltozás
								ninczen,</lem>
							<rdg wit="#W1" xml:id="rdg.8">változas</rdg>
							<rdg wit="#W2" xml:id="rdg.9">változás</rdg>
							<rdg wit="#W3" xml:id="rdg.10">valtozás</rdg>
						</app></l>
					<l xml:id="l.28">Az ö népét ö megmenté,</l>
					<l xml:id="l.29">Es ö vélec kötést szerze,</l>
					<l xml:id="l.30"><app xml:id="app.9">
							<lem wit="#KK" xml:id="lem.9">Ki megmarad minden idöben.</lem>
							<rdg wit="#W1" xml:id="rdg.11">üdöben</rdg>
						</app></l>
				</lg>
				<lg xml:id="lg.6">
					<fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
					<l xml:id="l.31"><app xml:id="app.10">
							<lem wit="#KK" xml:id="lem.10">Szent és diczö az ö neve,</lem>
							<rdg wit="#W1" xml:id="rdg.12">düczö</rdg>
						</app></l>
					<l xml:id="l.32">Es az Urnac ö félelme</l>
					<l xml:id="l.33">Az jo bölczeségnec kezdeti,</l>
					<l xml:id="l.34">Ki megtartya ö törvényét,</l>
					<l xml:id="l.35">Es megörzi szent igéjét,</l>
					<l xml:id="l.36">Annac megmarad diczéreti.</l>
				</lg>
			</div>
