    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 134.</fw>
                <head type="title">CXXXIV. SOLTAR. T. B.</head>
                <head type="sub">Intése az Papoknac, hogy tisztekben hiven eljárjanac.</head>
                <pb facs="305"/>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">URnac szolgai mindnyájan</l>
                    <l xml:id="l.2">Aldgyátoc az Urat vigan,</l>
                    <l xml:id="l.3">Kic az ö házában éyjel</l>
                    <l xml:id="l.4"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">Vigyázván vattoc hivséggel.</lem>
                            <rdg wit="#W1" xml:id="rdg.1">hüséggel</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.5">Fölemelvén kezeteket</l>
                    <l xml:id="l.6">Diczérjétec Istenteket,</l>
                    <l xml:id="l.7"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Szivböl néki
                                hál<corr>á</corr>tadvan,</lem>
                            <rdg wit="#W1 #W3" xml:id="rdg.2">hálat</rdg>
                            <rdg wit="#W2" xml:id="rdg.3">hálát</rdg>
                        </app></l>
                    <l xml:id="l.8">Öt áldgyátoc minduntalan.</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.9"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Megáldgyon téged
                                <corr>a</corr>zIsten</lem>
                            <rdg wit="#W2" xml:id="rdg.4"><sic>Mégdálgyon</sic></rdg>
                            <rdg wit="#W1 #W2" xml:id="rdg.5">az</rdg>
                            <rdg wit="#W3" xml:id="rdg.6">áz</rdg>
                        </app></l>
                    <l xml:id="l.10"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Az Sionról kegyelmessen,</lem>
                            <rdg wit="#W2" xml:id="rdg.7">Siónrol</rdg>
                        </app></l>
                    <l xml:id="l.11">Ki teremtette az eget,</l>
                    <l xml:id="l.12">Az földet és mindeneket.</l>
                </lg>
            </div>
        </body>
