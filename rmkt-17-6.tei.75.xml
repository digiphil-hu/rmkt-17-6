   <text>
      <body>
         <div type="poem" xml:id="d.1" xml:lang="hu">
            <fw type="originalNum">[RMKT XVII/6:] 75.</fw>
            <head type="title">LXXV. SOLTAR. T. B.</head>
            <head type="sub">Fogadás tétele Davidnac az ö következendö regnálásárol.</head>
            <pb facs="180"> </pb>
            <lg xml:id="lg.1">
               <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
               <l xml:id="l.1">DIczérünc téged Isten,</l>
               <l xml:id="l.2">Diczéret legyen néked,</l>
               <l xml:id="l.3"><app xml:id="app.1">
                     <lem wit="#KK" xml:id="lem.1">Mert az te diczö neved</lem>
                     <rdg wit="#W1" xml:id="rdg.1">düczö</rdg>
                  </app></l>
               <l xml:id="l.4">Hozzánc közel jött hiven,</l>
               <l xml:id="l.5">Mi azért czudáidat</l>
               <l xml:id="l.6">Hirdettyük jovoltodat.</l>
            </lg>
            <lg xml:id="lg.2">
               <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
               <l xml:id="l.7">Ha idöm lesz valaha</l>
               <l xml:id="l.8">En igazán itélec,</l>
               <l xml:id="l.9">Ha feneke ez földnec</l>
               <l xml:id="l.10">Egyszersmind lerohanna,</l>
               <l xml:id="l.11"><app xml:id="app.2">
                     <lem wit="#KK" xml:id="lem.2">En mégis ö oszlopát</lem>
                     <rdg wit="#W1 #W2" xml:id="rdg.2">oszlopját</rdg>
                  </app></l>
               <l xml:id="l.12"><app xml:id="app.3">
                     <lem wit="#KK" xml:id="lem.3">Jol állatnám gyámol<corr>á</corr>t.</lem>
                     <rdg wit="#W1" xml:id="rdg.3">gjámolát</rdg>
                     <rdg wit="#W2" xml:id="rdg.4">gyámolát</rdg>
                     <rdg wit="#W3" xml:id="rdg.5">gyámolat</rdg>
                  </app></l>
            </lg>
            <lg xml:id="lg.3">
               <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
               <l xml:id="l.13">Az bolondokat intem;</l>
               <l xml:id="l.14">Ne bolondoskodgyatoc,</l>
               <l xml:id="l.15"><app xml:id="app.4">
                     <lem wit="#KK" xml:id="lem.4">Az kic pedig gonoszoc,</lem>
                     <rdg wit="#W1" xml:id="rdg.6">penig</rdg>
                  </app></l>
               <l xml:id="l.16">Térjetec meg, ugy kértem,</l>
               <l xml:id="l.17">Szarvat ne emellyetec,</l>
               <l xml:id="l.18">Illy fön ne beszéllyetec.</l>
            </lg>
            <lg xml:id="lg.4">
               <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
               <l xml:id="l.19"><app xml:id="app.5">
                     <lem wit="#KK" xml:id="lem.5">Mert az tiszt es az erö</lem>
                     <rdg wit="#W2" xml:id="rdg.7">és</rdg>
                  </app></l>
               <l xml:id="l.20">Nem az napkelet felöl</l>
               <l xml:id="l.21">Sem az nap enyészetröl</l>
               <l xml:id="l.22">Sem nem az pusztából jö,</l>
               <l xml:id="l.23">Minden az Istenen ál,</l>
               <l xml:id="l.24">Ö aláz s’ ö magasztal.</l>
            </lg>
            <lg xml:id="lg.5">
               <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
               <l xml:id="l.25">Az pohár ö kezében,</l>
               <l xml:id="l.26">Kiben vagyon vörös bor,</l>
               <l xml:id="l.27">Kit ö teli tölt sokszor</l>
               <l xml:id="l.28">Minden népnec ez földen,</l>
               <l xml:id="l.29">De végre az gonoszszac</l>
               <l xml:id="l.30">Az söprejét megiszszác.</l>
            </lg>
            <lg xml:id="lg.6">
               <fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
               <l xml:id="l.31">En öröcké diczérem</l>
               <l xml:id="l.32"><app xml:id="app.6">
                     <lem wit="#KK" xml:id="lem.6">Az Jacobnac Istenét,</lem>
                     <rdg wit="#W2" xml:id="rdg.8">Isténét</rdg>
                  </app></l>
               <l xml:id="l.33"><app xml:id="app.7">
                     <lem wit="#KK" xml:id="lem.7">Hirdetem diczöségét,</lem>
                     <rdg wit="#W1" xml:id="rdg.9">düczöségét</rdg>
                  </app></l>
               <l xml:id="l.34">Es szarvokat megszegem</l>
               <l xml:id="l.35">Az istenteleneknec,</l>
               <l xml:id="l.36">Hogy az jóc fölkellyenec.</l>
            </lg>
         </div>
