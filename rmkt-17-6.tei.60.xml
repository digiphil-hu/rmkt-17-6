    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 60.</fw>
                <head type="title">LX. SOLTAR. T. B. 108.</head>
                <head type="sub">Könyörgése Davidnac, az Izrael népénec jo állapattyáért.</head>
                <pb facs="146"> </pb>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">MInket Ur Isten elhagyál,</l>
                    <l xml:id="l.2">Es reánc megharaguttál,</l>
                    <l xml:id="l.3">Töled széllel eloszlatánc,</l>
                    <l xml:id="l.4"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">T<corr>é</corr>rj kegyessen
                                isméthozzánc,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.1">Térj</rdg>
                            <rdg wit="#W3" xml:id="rdg.2">Terj</rdg>
                        </app></l>
                    <l rend="level_1" xml:id="l.5">Mind ez földet meginditád,</l>
                    <l xml:id="l.6"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Nagy
                                hatalma<corr>d</corr>dalmegrontád,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.3">hatalmaddal</rdg>
                            <rdg wit="#W3" xml:id="rdg.4">hatalmadal</rdg>
                        </app></l>
                    <l xml:id="l.7">Epits meg az ö nagy romlását,</l>
                    <l xml:id="l.8">Mert romlás miatt alég álhat.</l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.9">Népedet keményen tartád,</l>
                    <l xml:id="l.10">Es igen megsanyargatád,</l>
                    <l xml:id="l.11">Minket mint egy czipös borral,</l>
                    <l xml:id="l.12">Itatál keserü buval,</l>
                    <l rend="level_1" xml:id="l.13">De az kic tégedet félnec,</l>
                    <l xml:id="l.14">Az zászlót adád nékiec,</l>
                    <l xml:id="l.15"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Mellyet ök fölemelnec bizvan</lem>
                            <rdg wit="#W2" xml:id="rdg.5">bizván</rdg>
                        </app></l>
                    <l xml:id="l.16">Az te igaz fogadásodban.</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.17">Hogy az nálad kedves hivec</l>
                    <l xml:id="l.18"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">V<corr>é</corr>gre megmenekedgyenec,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.6">Végre</rdg>
                            <rdg wit="#W3" xml:id="rdg.7">Vegre</rdg>
                        </app></l>
                    <l xml:id="l.19">Job kezeddel tarts meg engem,</l>
                    <l xml:id="l.20"><app xml:id="app.5">
                            <lem wit="#KK" xml:id="lem.5">Halgass meg es tégy jol velem.</lem>
                            <rdg wit="#W2" xml:id="rdg.8">és</rdg>
                        </app></l>
                    <l rend="level_1" xml:id="l.21">Szola Isten szentségében,</l>
                    <l xml:id="l.22"><app xml:id="app.6">
                            <lem wit="#KK" xml:id="lem.6">Örvendec ig<corr>é</corr>retiben,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.9">igéretiben</rdg>
                            <rdg wit="#W3" xml:id="rdg.10">igeretiben</rdg>
                        </app></l>
                    <l xml:id="l.23">Az Sichemet részül elveszem,</l>
                    <l xml:id="l.24">Az Suchot völgyét megmértéklem.</l>
                </lg>
                <lg xml:id="lg.4">
                    <metamark function="pause"/>
                    <fw corresp="Szenci Molnár Albert" type="lg"><hi rend="super">*</hi><hi
                            rend="sub">*</hi><hi rend="super">*</hi> 4 </fw>
                    <l xml:id="l.25">Galaádotis ezennel</l>
                    <l xml:id="l.26"><app xml:id="app.7">
                            <lem wit="#KK" xml:id="lem.7">Megveszem erösé<corr>g</corr>emmel,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.11">eröségemmel</rdg>
                            <rdg wit="#W3" xml:id="rdg.12">eröséggemmel</rdg>
                        </app></l>
                    <l xml:id="l.27">Az Manassesis ecképpen,</l>
                    <l xml:id="l.28"><app xml:id="app.8">
                            <lem wit="#KK" xml:id="lem.8">Ephraimis enyim leszen.</lem>
                            <rdg wit="#W1" xml:id="rdg.13">ennyim</rdg>
                        </app></l>
                    <l rend="level_1" xml:id="l.29">Ezec az nagy sereg népec</l>
                    <l xml:id="l.30">Fejemnec ereje lesznec,</l>
                    <l xml:id="l.31">Az Judais én örökségem</l>
                    <l xml:id="l.32"><app xml:id="app.9">
                            <lem wit="#KK" xml:id="lem.9">Es törv<corr>é</corr>nszerzöm
                                leszennékem.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.14">törvénszerzöm</rdg>
                            <rdg wit="#W3" xml:id="rdg.15">törvenszerzöm</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.5">
                    <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                    <l xml:id="l.33">Az Moábis az én népem,</l>
                    <l xml:id="l.34"><app xml:id="app.10">
                            <lem wit="#KK" xml:id="lem.10">Mint egy
                                lábmosóed<corr>é</corr>nyem</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.16">edényem</rdg>
                            <rdg wit="#W3" xml:id="rdg.17">edenyem</rdg>
                        </app></l>
                    <l xml:id="l.35"><app xml:id="app.11">
                            <lem wit="#KK" xml:id="lem.11">Kiben l<corr>á</corr>baimat mosom,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.18">lábaimat</rdg>
                            <rdg wit="#W3" xml:id="rdg.19">labaimat</rdg>
                        </app></l>
                    <l xml:id="l.36"><app xml:id="app.12">
                            <lem wit="#KK" xml:id="lem.12">Eb<corr>b</corr>en én
                                nemgyanakodom,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.20">Ebben</rdg>
                            <rdg wit="#W3" xml:id="rdg.21">Eben</rdg>
                        </app></l>
                    <l rend="level_1" xml:id="l.37">Es azt én nyilván elhiszem</l>
                    <l xml:id="l.38">Edomra sarumot vetem,</l>
                    <l xml:id="l.39">Es kic Philistéaban laktoc</l>
                    <l xml:id="l.40">Gyözödelmemen vigadoztoc.</l>
                </lg>
                <lg xml:id="lg.6">
                    <fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
                    <l xml:id="l.41">Ki vihetne engem bátran</l>
                    <l xml:id="l.42">Az nagy kerétett városban?</l>
                    <l xml:id="l.43">Utamat ki igazgatná</l>
                    <l xml:id="l.44">Az erös Edom országra?</l>
                    <l rend="level_1" xml:id="l.45">Nem te tészedé Ur Isten,</l>
                    <l xml:id="l.46"><app xml:id="app.13">
                            <lem wit="#KK" xml:id="lem.13">Ki minket üzsz
                                helyb<corr>ö</corr>lhelyben?</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.22">helyböl</rdg>
                            <rdg wit="#W3" xml:id="rdg.23">helyból</rdg>
                        </app></l>
                    <l xml:id="l.47">Hogy el nem jövél sereginckel,</l>
                    <l xml:id="l.48">Megüzetél ellenséginckel.</l>
                </lg>
                <lg xml:id="lg.7">
                    <fw corresp="Szenci Molnár Albert" type="lg">7 </fw>
                    <l xml:id="l.49"><app xml:id="app.14">
                            <lem wit="#KK" xml:id="lem.14">Légy
                                mineküncs<corr>e</corr>gétségöl,</lem>
                            <rdg wit="#W1" xml:id="rdg.24">ségedségöl</rdg>
                            <rdg wit="#W2" xml:id="rdg.25">ségetségöl</rdg>
                            <rdg wit="#W3" xml:id="rdg.26">ségétségöl</rdg>
                        </app></l>
                    <l xml:id="l.50">Örizzmeg ellenségünktöl,</l>
                    <l xml:id="l.51">Mert az emberi segétség,</l>
                    <l xml:id="l.52">Hijában valo epesség.</l>
                    <l rend="level_1" xml:id="l.53">Az Isten által minékünc</l>
                    <l xml:id="l.54">Lészen erös gyözödelmünc,</l>
                    <l xml:id="l.55">Es ö megszabadit bennünket,</l>
                    <l xml:id="l.56">Megtapodgya ellenséginket.</l>
                </lg>
            </div>
