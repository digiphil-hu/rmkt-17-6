        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 136.</fw>
                <head type="title">CXXXVI. SOLTAR. T. B.</head>
                <head type="sub"><app xml:id="app.1">
                        <lem wit="#KK" xml:id="lem.1">Intés az Isten
                                dicz<corr>é</corr>r<corr>e</corr>tire, az ö kijelentett diczösegiert
                            és jo tétemenyiért.</lem>
                        <rdg wit="#W1" xml:id="rdg.1">diczeretire</rdg>
                        <rdg wit="#W2 #W3" xml:id="rdg.2">diczerétire</rdg>
                        <rdg wit="#W1" xml:id="rdg.3">düczösegiert</rdg>
                    </app></head>
                <pb facs="308"/>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">DIczérjétec az Urat</l>
                    <l xml:id="l.2">Mert ö jo kedvet mutat,</l>
                    <l xml:id="l.3">Es az ö kegyessége</l>
                    <l xml:id="l.4">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.5">Aldgyátoc Istenteket,</l>
                    <l xml:id="l.6"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Isteneknec Istenét,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.4">Istenöknec</rdg>
                        </app></l>
                    <l xml:id="l.7">Mert az ö kegyessége</l>
                    <l xml:id="l.8">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.9">Diczérje ötet minden,</l>
                    <l xml:id="l.10">Mert nagy czudákat tészen,</l>
                    <l xml:id="l.11">Es az ö kegyessége</l>
                    <l xml:id="l.12">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.4">
                    <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                    <l xml:id="l.13">Minden öt magasztallya,</l>
                    <l xml:id="l.14">Mert ö Uraknac Ura,</l>
                    <l xml:id="l.15">Es az ö kegyessége</l>
                    <l xml:id="l.16">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.5">
                    <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                    <l xml:id="l.17">Ki az mennyet teremté</l>
                    <l xml:id="l.18">Es bölczen ékesité,</l>
                    <l xml:id="l.19">Mert az ö kegyessége</l>
                    <l xml:id="l.20"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Megmarad mind öröck<corr>e</corr>.</lem>
                            <rdg wit="#W1" xml:id="rdg.5">örökke</rdg>
                            <rdg wit="#W2 #W3" xml:id="rdg.6">öröcké</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.6">
                    <fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
                    <l xml:id="l.21">Az ki az széles földet</l>
                    <l xml:id="l.22">Szerzette az viz fölet,</l>
                    <l xml:id="l.23">Es az ö kegyessége</l>
                    <l xml:id="l.24">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.7">
                    <fw corresp="Szenci Molnár Albert" type="lg">7 </fw>
                    <l xml:id="l.25">Ki nagy szép lámpásokot</l>
                    <l xml:id="l.26">Fön az égen alkotot,</l>
                    <l xml:id="l.27">Es az ö kegyessége</l>
                    <l xml:id="l.28">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.8">
                    <fw corresp="Szenci Molnár Albert" type="lg">8 </fw>
                    <l xml:id="l.29">Nappali vezérlésént</l>
                    <l xml:id="l.30">Teremtötte az napfént,</l>
                    <l xml:id="l.31">Es az ö kegyessége</l>
                    <l xml:id="l.32"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Megm<corr>a</corr>rad min<corr>d</corr>
                                öröcke.</lem>
                            <rdg wit="#W1" xml:id="rdg.7">Megmarad mind</rdg>
                            <rdg wit="#W2" xml:id="rdg.8">Megmarad mint</rdg>
                            <rdg wit="#W3" xml:id="rdg.9">Megmárad mint</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.9">
                    <fw corresp="Szenci Molnár Albert" type="lg">9 </fw>
                    <l xml:id="l.33">Holdot, czillag sereget</l>
                    <l xml:id="l.34">Az étzakára szerzet,</l>
                    <l xml:id="l.35">Es az ö kegyessége</l>
                    <l xml:id="l.36"><app xml:id="app.5">
                            <lem wit="#KK" xml:id="lem.5">M<corr>e</corr>gmarad mind öröcke.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.10">Megmarad</rdg>
                            <rdg wit="#W3" xml:id="rdg.11">Mégmarad</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.10">
                    <fw corresp="Szenci Molnár Albert" type="lg">10 </fw>
                    <l xml:id="l.37">Ki elsö szülötteket</l>
                    <l xml:id="l.38">Egyiptomban elvesztet,</l>
                    <l xml:id="l.39">Es az ö kegyessége</l>
                    <l xml:id="l.40">Megmarad mind öröcke,</l>
                </lg>
                <lg xml:id="lg.11">
                    <fw corresp="Szenci Molnár Albert" type="lg">11 </fw>
                    <l xml:id="l.41">Es az Izraelt végre</l>
                    <l xml:id="l.42">Innét más helyre vitte,</l>
                    <l xml:id="l.43">Es az ö kegyessége</l>
                    <l xml:id="l.44">Megmarad mind öröcke</l>
                </lg>
                <lg xml:id="lg.12">
                    <fw corresp="Szenci Molnár Albert" type="lg">12 </fw>
                    <l xml:id="l.45">Az ö nagy erejével</l>
                    <l xml:id="l.46">Azt téve jobkezével,</l>
                    <l xml:id="l.47">Es az ö kegyessége</l>
                    <l xml:id="l.48">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.13">
                    <fw corresp="Szenci Molnár Albert" type="lg">13 </fw>
                    <l xml:id="l.49">Az vörös tengert menten</l>
                    <l xml:id="l.50">Ketté választa könnyen,</l>
                    <l xml:id="l.51">Es az ö kegyessége</l>
                    <l xml:id="l.52">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.14">
                    <fw corresp="Szenci Molnár Albert" type="lg">14 </fw>
                    <l xml:id="l.53"><app xml:id="app.6">
                            <lem wit="#KK" xml:id="lem.6">Altal vez<corr>e</corr>té hiven</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.12">vezeté</rdg>
                            <rdg wit="#W3" xml:id="rdg.13">vezété</rdg>
                        </app></l>
                    <l xml:id="l.54">Izraelt az tengeren,</l>
                    <l xml:id="l.55">Es az ö kegyessége</l>
                    <l xml:id="l.56">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.15">
                    <fw corresp="Szenci Molnár Albert" type="lg">15 </fw>
                    <l xml:id="l.57">Pharaot az tengerben</l>
                    <l xml:id="l.58">Veszté népével egyben,</l>
                    <l xml:id="l.59">Es az ö kegyessége</l>
                    <l xml:id="l.60">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.16">
                    <metamark function="pause"/>
                    <fw corresp="Szenci Molnár Albert" type="lg"><hi rend="super">*</hi><hi
                            rend="sub">*</hi><hi rend="super">*</hi> 16 </fw>
                    <l xml:id="l.61"><app xml:id="app.7">
                            <lem wit="#KK" xml:id="lem.7">Népét vez<corr>é</corr>rlé hiven</lem>
                            <rdg wit="#W1" xml:id="rdg.14">vezérlé</rdg>
                            <rdg wit="#W2" xml:id="rdg.15">vezétlé</rdg>
                            <rdg wit="#W3" xml:id="rdg.16">vezerlé</rdg>
                        </app></l>
                    <l xml:id="l.62">Az szörnyü kietlenben,</l>
                    <l xml:id="l.63">Es az ö kegyessége</l>
                    <l xml:id="l.64">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.17">
                    <fw corresp="Szenci Molnár Albert" type="lg">17 </fw>
                    <l xml:id="l.65">Az királyokat széllel</l>
                    <l xml:id="l.66">Megrontá kezeivel,</l>
                    <l xml:id="l.67">Es az ö kegyessége</l>
                    <l xml:id="l.68">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.18">
                    <fw corresp="Szenci Molnár Albert" type="lg">18 </fw>
                    <l xml:id="l.69">Erös fejedelmeket</l>
                    <l xml:id="l.70">Ö megölt és elvesztet,</l>
                    <l xml:id="l.71">Es az ö kegyessége</l>
                    <l xml:id="l.72">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.19">
                    <fw corresp="Szenci Molnár Albert" type="lg">19 </fw>
                    <l xml:id="l.73">Az Amornac földébe</l>
                    <l xml:id="l.74">Sihon királt elveszté,</l>
                    <l xml:id="l.75">Es az ö kegyessége</l>
                    <l xml:id="l.76">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.20">
                    <fw corresp="Szenci Molnár Albert" type="lg">20 </fw>
                    <l xml:id="l.77">Az Og királt Basanban</l>
                    <l xml:id="l.78">Megölé hatalmában,</l>
                    <l xml:id="l.79"><app xml:id="app.8">
                            <lem wit="#KK" xml:id="lem.8">Es az ö kegy<corr>e</corr>ssége</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.17">kegyessége</rdg>
                            <rdg wit="#W3" xml:id="rdg.18">kegyéssége</rdg>
                        </app></l>
                    <l xml:id="l.80">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.21">
                    <fw corresp="Szenci Molnár Albert" type="lg">21 </fw>
                    <l xml:id="l.81">Es földöket ezeknec</l>
                    <l xml:id="l.82">Adá az ö népénec,</l>
                    <l xml:id="l.83">Es az ö kegyessége</l>
                    <l xml:id="l.84">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.22">
                    <fw corresp="Szenci Molnár Albert" type="lg">22 </fw>
                    <l xml:id="l.85">Hogy Izrael népénec</l>
                    <l xml:id="l.86">Ott lenne örökségec,</l>
                    <l xml:id="l.87">Es az ö kegyessége</l>
                    <l xml:id="l.88">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.23">
                    <fw corresp="Szenci Molnár Albert" type="lg">23 </fw>
                    <l xml:id="l.89">Megemleközéc rólunc</l>
                    <l xml:id="l.90"><app xml:id="app.9">
                            <lem wit="#KK" xml:id="lem.9">Látván nyomor<corr>ú</corr>ságunc,</lem>
                            <rdg wit="#W1" xml:id="rdg.19">nyomoruságunc</rdg>
                            <rdg wit="#W2" xml:id="rdg.20">nyomorságunc</rdg>
                            <rdg wit="#W3" xml:id="rdg.21">nyomorúiságunc</rdg>
                        </app></l>
                    <l xml:id="l.91">Es az ö kegyessége</l>
                    <l xml:id="l.92">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.24">
                    <fw corresp="Szenci Molnár Albert" type="lg">24 </fw>
                    <l xml:id="l.93">Megmente jokedvéböl</l>
                    <l xml:id="l.94">Ellenségünc kezéböl,</l>
                    <l xml:id="l.95">Es az ö kegyessége</l>
                    <l xml:id="l.96">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.25">
                    <fw corresp="Szenci Molnár Albert" type="lg">25 </fw>
                    <l xml:id="l.97">Ez földön minden testet</l>
                    <l xml:id="l.98"><app xml:id="app.10">
                            <lem wit="#KK" xml:id="lem.10">T<corr>á</corr>plál es böven éltet,</lem>
                            <rdg wit="#W1" xml:id="rdg.22">Táplál</rdg>
                            <rdg wit="#W2 #W3" xml:id="rdg.23">Tálplál</rdg>
                        </app></l>
                    <l xml:id="l.99">Es az ö kegyessége</l>
                    <l xml:id="l.100">Megmarad mind öröcke.</l>
                </lg>
                <lg xml:id="lg.26">
                    <fw corresp="Szenci Molnár Albert" type="lg">26 </fw>
                    <l xml:id="l.101">Aldgyátoc az Ur Istent</l>
                    <l xml:id="l.102">Minden népec naponként,</l>
                    <l xml:id="l.103">Mert az ö kegyessége</l>
                    <l xml:id="l.104">Megmarad mind öröcke.</l>
                </lg>
            </div>
