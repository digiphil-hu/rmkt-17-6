	<text>
		<body>
			<div type="poem" xml:id="d.1" xml:lang="hu">
				<fw type="originalNum">[RMKT XVII/6:] 129.</fw>
				<head type="title">CXXIX. SOLTAR. T. B.</head>
				<head type="sub">Diczéreti az Istennec az megszabaditásért.</head>
				<pb facs="299"/>
				<lg xml:id="lg.1">
					<fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
					<l xml:id="l.1">EN ifjuságomtul fogva engem</l>
					<l xml:id="l.2">Sanyargattac, mondhattya az Izrael,</l>
					<l xml:id="l.3"><app xml:id="app.1">
							<lem wit="#KK" xml:id="lem.1">R<corr>é</corr>gtül fogva soc bút töttec
								nékem,</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.1">Régtül</rdg>
							<rdg wit="#W3" xml:id="rdg.2">Regtül</rdg>
						</app></l>
					<l xml:id="l.4">Még sem fogyathattac el tellyességgel.</l>
				</lg>
				<lg xml:id="lg.2">
					<fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
					<l xml:id="l.5"><app xml:id="app.2">
							<lem wit="#KK" xml:id="lem.2">Keresztül széllyel az én hátamat</lem>
							<rdg wit="#W1" xml:id="rdg.3">széllel</rdg>
						</app></l>
					<l xml:id="l.6">Megszántottác és megszaggattác szörnyen,</l>
					<l xml:id="l.7">Vontanac raytam nagy barázdákat,</l>
					<l xml:id="l.8">Ugy hogy testemben semmi ép tag ninczen.</l>
				</lg>
				<lg xml:id="lg.3">
					<fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
					<l xml:id="l.9">De az igasságnac jo Istene</l>
					<l xml:id="l.10">Elszaggatá az ellenség kötelét,</l>
					<l xml:id="l.11">Kic irégykednec Sion hegyére,</l>
					<l xml:id="l.12">Nagy szégyenség téritse hátra öket.</l>
				</lg>
				<lg xml:id="lg.4">
					<fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
					<l xml:id="l.13">Mint az hitván fü ollyac legyenec,</l>
					<l xml:id="l.14">Az melly szokott nöni az eszterhákon,</l>
					<l xml:id="l.15"><app xml:id="app.3">
							<lem wit="#KK" xml:id="lem.3">Elasz minekelötte kinyönéc,</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.4">Elâszsz</rdg>
						</app></l>
					<l xml:id="l.16"><app xml:id="app.4">
							<lem wit="#KK" xml:id="lem.4">Az kiben ninczen sohol semmi haszon.</lem>
							<rdg wit="#W1" xml:id="rdg.5">haszón</rdg>
						</app></l>
				</lg>
				<lg xml:id="lg.5">
					<fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
					<l xml:id="l.17">Kin az arató nem talál annyét</l>
					<l xml:id="l.18">Hogy egy marockal valót arathatna,</l>
					<l xml:id="l.19">Nem hogy köthetne valami kévét,</l>
					<l xml:id="l.20">Az kit ember ölében szorithatna.</l>
				</lg>
				<lg xml:id="lg.6">
					<fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
					<l xml:id="l.21"><app xml:id="app.5">
							<lem wit="#KK" xml:id="lem.5">Es az kic elmennec, ne
									mondgy<corr>á</corr>c,</lem>
							<rdg wit="#W1" xml:id="rdg.6">mongyák</rdg>
							<rdg wit="#W2" xml:id="rdg.7">mongyác</rdg>
							<rdg wit="#W3" xml:id="rdg.8">mondgyac</rdg>
						</app></l>
					<l xml:id="l.22"><app xml:id="app.6">
							<lem wit="#KK" xml:id="lem.6">Az kegyes Isten áldgyon meg
								<corr>t</corr>iteket,</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.9">meg titeket</rdg>
							<rdg wit="#W3" xml:id="rdg.10">megtiteket</rdg>
						</app></l>
					<l xml:id="l.23">Aratástoc legyenec szaporác,</l>
					<l xml:id="l.24"><app xml:id="app.7">
							<lem wit="#KK" xml:id="lem.7">Az Ur nevében áldunc benneteket.</lem>
							<rdg wit="#W1" xml:id="rdg.11">neviben</rdg>
						</app></l>
				</lg>
			</div>
		</body>
	</text>
