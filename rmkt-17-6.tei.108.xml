                        <div type="poem" xml:id="d.1" xml:lang="hu">
                                <fw type="originalNum">[RMKT XVII/6:] 108.</fw>
                                <head type="title">CVIII. SOLTAR. T. B. 60.</head>
                                <head type="sub"><app xml:id="app.1">
                                                <lem wit="#KK" xml:id="lem.1">Könyörgése Davidnac az
                                                  ö királyságánac egéz és tellyes
                                                  birodalmaért.</lem>
                                                <rdg wit="#W1 #W2" xml:id="rdg.1">téllyes</rdg>
                                        </app></head>
                                <head type="notation">Az LX. Soltar notajára.</head>
                                <lg xml:id="lg.1">
                                        <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                                        <l xml:id="l.1"><app xml:id="app.2">
                                                  <lem wit="#KK" xml:id="lem.2">UR Isten kész az én
                                                  szivem,</lem>
                                                  <rdg wit="#W1" xml:id="rdg.2">áz</rdg>
                                                </app></l>
                                        <l xml:id="l.2">Es azon vagyon én lelkem</l>
                                        <l xml:id="l.3">Hogy tenéked énekellyen,</l>
                                        <l xml:id="l.4">Hegedöben zengedezzen,</l>
                                        <l xml:id="l.5">Nosza lantoc és citerác</l>
                                        <l xml:id="l.6"><app xml:id="app.3">
                                                  <lem wit="#KK" xml:id="lem.3">Zendüllyetec föl
                                                  musikác,</lem>
                                                  <rdg wit="#W1" xml:id="rdg.3">Zöndüllyetek</rdg>
                                                </app></l>
                                        <l xml:id="l.7">Mert igyekezem én szivemben</l>
                                        <l xml:id="l.8">Igy menni az Ur eleiben.</l>
                                </lg>
                                <lg xml:id="lg.2">
                                        <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                                        <l xml:id="l.9">Diczérlec Uram tégedöt,</l>
                                        <l xml:id="l.10">Minden nemzetségec elöt,</l>
                                        <l xml:id="l.11">Tisztellec szép énekeckel</l>
                                        <l xml:id="l.12"><app xml:id="app.4">
                                                  <lem wit="#KK" xml:id="lem.4">Minden nép elött vig
                                                  szivvel,</lem>
                                                  <rdg wit="#W1 #W2" xml:id="rdg.4">szivel</rdg>
                                                </app></l>
                                        <l xml:id="l.13">Mert az te kegyelmességed</l>
                                        <l xml:id="l.14">Az széles égre kiterjed</l>
                                        <l xml:id="l.15">Fölségednec szent igassága</l>
                                        <l xml:id="l.16">Az fölhököt mind föllyölmullya.</l>
                                </lg>
                                <lg xml:id="lg.3">
                                        <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                                        <l xml:id="l.17"><app xml:id="app.5">
                                                  <lem wit="#KK" xml:id="lem.5">Magasztaltassál az
                                                  égre,</lem>
                                                  <rdg wit="#W1 #W2" xml:id="rdg.5"
                                                  >Magasztalltassál</rdg>
                                                </app></l>
                                        <l xml:id="l.18"><app xml:id="app.6">
                                                  <lem wit="#KK" xml:id="lem.6">Diczösséged nyuyts
                                                  ez <corr>f</corr>öldre,</lem>
                                                  <rdg wit="#W1" xml:id="rdg.6">Düczösséged</rdg>
                                                  <rdg wit="#W1 #W2" xml:id="rdg.7">ez földre</rdg>
                                                  <rdg wit="#W3" xml:id="rdg.8">ezföldre</rdg>
                                                </app></l>
                                        <l xml:id="l.19">Szerelmes népedet tartsmeg,</l>
                                        <l xml:id="l.20">Nyuyts kezed, engem halgass meg.</l>
                                        <l xml:id="l.21">Az Isten ö szentségében</l>
                                        <l xml:id="l.22">Megvigaztala kegyessen,</l>
                                        <l xml:id="l.23">Hogy az Sichemet én megveszem,</l>
                                        <l xml:id="l.24">Es az Sucot völgyét elmérem.</l>
                                </lg>
                                <lg xml:id="lg.4">
                                        <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                                        <l xml:id="l.25">Az Galádotis acképpen</l>
                                        <l xml:id="l.26">Megveszem én nagy erömben,</l>
                                        <l xml:id="l.27">Az Manassesis mind enyim,</l>
                                        <l xml:id="l.28">Azonképpen az Ephraim.</l>
                                        <l xml:id="l.29">Mind ez seregec enyimec,</l>
                                        <l xml:id="l.30">Erösségec én fejemnec,</l>
                                        <l xml:id="l.31">Juda lesz királyi országom,</l>
                                        <l xml:id="l.32">Es én törvény szerzö regnálóm.</l>
                                </lg>
                                <lg xml:id="lg.5">
                                        <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                                        <l xml:id="l.33">Az Moab leszen énnekem</l>
                                        <l xml:id="l.34">Mint egy lábmosó edényem,</l>
                                        <l xml:id="l.35"><app xml:id="app.7">
                                                  <lem wit="#KK" xml:id="lem.7">Kiben
                                                  l<corr>á</corr>baimat mosom,</lem>
                                                  <rdg wit="#W1 #W2" xml:id="rdg.9">lábaimat</rdg>
                                                  <rdg wit="#W3" xml:id="rdg.10">labaimat</rdg>
                                                </app></l>
                                        <l xml:id="l.36">Es azt én bizonnyal tudom,</l>
                                        <l xml:id="l.37">Es tellyességgel elhiszem,</l>
                                        <l xml:id="l.38">Edomra sarumot vetem,</l>
                                        <l xml:id="l.39">Philistéusoc örvendgyetec</l>
                                        <l xml:id="l.40"><app xml:id="app.8">
                                                  <lem wit="#KK" xml:id="lem.8">Az én diczö
                                                  gyözödelmemnec.</lem>
                                                  <rdg wit="#W1" xml:id="rdg.11">düczö</rdg>
                                                </app></l>
                                </lg>
                                <lg xml:id="lg.6">
                                        <fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
                                        <l xml:id="l.41">Ki vinne engemet bátran</l>
                                        <l xml:id="l.42"><app xml:id="app.9">
                                                  <lem wit="#KK" xml:id="lem.9">Ez nagy keritett
                                                  városban?</lem>
                                                  <rdg wit="#W1" xml:id="rdg.12">várasban</rdg>
                                                </app></l>
                                        <l xml:id="l.43">Engemet ki vezérlene</l>
                                        <l xml:id="l.44"><app xml:id="app.10">
                                                  <lem wit="#KK" xml:id="lem.10">Az erös Edom
                                                  földére<corr>?</corr></lem>
                                                  <rdg wit="#W1 #W2 #W3" xml:id="rdg.13"
                                                  >földére.</rdg>
                                                </app><note type="critic" xml:id="note.1">Kérdőjel
                                                  kell, vö. <persName>Lobwasser<idno
                                                  corresp="Ambrosius Lobwasser"/></persName>: Wer
                                                  leitet mich, damit ich kom Jn das gewaltig land
                                                  Edom?</note></l>
                                        <l xml:id="l.45">Nem te tészedé Ur Isten,</l>
                                        <l xml:id="l.46">Ki minket üzsz helyböl helyben?</l>
                                        <l xml:id="l.47">Hogy el nem jövél sereginckel</l>
                                        <l xml:id="l.48">Megüzetél ellenséginckel.</l>
                                </lg>
                                <lg xml:id="lg.7">
                                        <fw corresp="Szenci Molnár Albert" type="lg">7 </fw>
                                        <l xml:id="l.49">Légy mi nékünc segédsegöl,</l>
                                        <l xml:id="l.50">Örizz meg ellenségünktöl,</l>
                                        <l xml:id="l.51"><app xml:id="app.11">
                                                  <lem wit="#KK" xml:id="lem.11">Mert az emberi
                                                  segétség</lem>
                                                  <rdg wit="#W1" xml:id="rdg.14">segédség</rdg>
                                                </app></l>
                                        <l xml:id="l.52"><app xml:id="app.12">
                                                  <lem wit="#KK" xml:id="lem.12">Hiàban valo
                                                  epesség.</lem>
                                                  <rdg wit="#W1 #W2" xml:id="rdg.15">epeség</rdg>
                                                </app></l>
                                        <l xml:id="l.53">Az Isten által minékünc</l>
                                        <l xml:id="l.54">Lészen erös gyözödelmünc,</l>
                                        <l xml:id="l.55">Es ö megszabadit bennünket,</l>
                                        <l xml:id="l.56">Megtapodgya ellenségünket.</l>
                                </lg>
                        </div>
