    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 151.</fw>
                <head type="title">AZ ISTENNEK TIZPARANCZOLATTYA</head>
                <head type="sub">Exod. XX. C. M.</head>
                <head type="notation">Az CXL. Soltar notajara.</head>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">EMeldföl szived, füled nyisdmeg,</l>
                    <l xml:id="l.2"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">Te k<corr>e</corr>mény nyako Izrael,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.1">kemény</rdg>
                            <rdg wit="#W3" xml:id="rdg.2">kémény</rdg>
                        </app></l>
                    <l xml:id="l.3">Isten paranczolattyát érts meg.</l>
                    <l xml:id="l.4"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Törvényére figyelmezzél.</lem>
                            <rdg wit="#W1" xml:id="rdg.3">Törvényire</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.5">En vagyoc Istened egyedöl,</l>
                    <l xml:id="l.6"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Ki az Egyptombol téged,</lem>
                            <rdg wit="#W1" xml:id="rdg.4">az szolgalatbol</rdg>
                        </app></l>
                    <l xml:id="l.7">Kihoztalac nagy inségedböl,</l>
                    <l xml:id="l.8">Ne legyenec más Istenid.</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.9">Ne czinálly semmi bálvány képet,</l>
                    <l xml:id="l.10">Akarminémü dologbol,</l>
                    <l xml:id="l.11">Ne imádgyad se tiszteld öket,</l>
                    <l xml:id="l.12">Mert nagy haragom megindol.</l>
                </lg>
                <lg xml:id="lg.4">
                    <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                    <l xml:id="l.13">Uradnac Istenednec nevét</l>
                    <l xml:id="l.14"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Soha névegyed hiáb<corr>a</corr>n,</lem>
                            <rdg wit="#W1" xml:id="rdg.5">hiában</rdg>
                            <rdg wit="#W2 #W3" xml:id="rdg.6">hiábán</rdg>
                        </app></l>
                    <l xml:id="l.15">Mert az ki nem böczülli nevét,</l>
                    <l xml:id="l.16">Megbünteti azt valóban.</l>
                </lg>
                <lg xml:id="lg.5">
                    <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                    <l xml:id="l.17">Hat nap minden dolgodat tegyed,</l>
                    <l xml:id="l.18">Nyugodgyál az hetediken,</l>
                    <l xml:id="l.19">Mert hogy teremte mindeneket,</l>
                    <l xml:id="l.20">Ez nap megnyugovéc Isten.</l>
                </lg>
                <lg xml:id="lg.6">
                    <fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
                    <l xml:id="l.21"><app xml:id="app.5">
                            <lem wit="#KK" xml:id="lem.5">Az te atyádat, és anyádat,</lem>
                            <rdg wit="#W1" xml:id="rdg.7">attyádat</rdg>
                        </app></l>
                    <l xml:id="l.22">Tisztellyed szeressed hiven,</l>
                    <l xml:id="l.23">Hogy hoszszu életet, soc jokat</l>
                    <l xml:id="l.24">Adgyon tenéked az Isten.</l>
                </lg>
                <lg xml:id="lg.7">
                    <fw corresp="Szenci Molnár Albert" type="lg">7 </fw>
                    <l xml:id="l.25">Embert ne ölly, ne tégy vér ontást,</l>
                    <l xml:id="l.26"><app xml:id="app.6">
                            <lem wit="#KK" xml:id="lem.6">Paráznaságban ne lélegy,</lem>
                            <rdg wit="#W2" xml:id="rdg.8">lelégy</rdg>
                        </app></l>
                    <l xml:id="l.27">Ne orozz, gyülöllyed az lopást,</l>
                    <l xml:id="l.28">Hamis bizonságot ne tégy,</l>
                </lg>
                <lg xml:id="lg.8">
                    <fw corresp="Szenci Molnár Albert" type="lg">8 </fw>
                    <l xml:id="l.29">Felebarátodnac ö házat</l>
                    <l xml:id="l.30"><app xml:id="app.7">
                            <lem wit="#KK" xml:id="lem.7">Ne kivánd, se
                                h<corr>á</corr>zastársat,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.9">házas</rdg>
                            <rdg wit="#W3" xml:id="rdg.10">hazas</rdg>
                        </app></l>
                    <l xml:id="l.31">Szolgáját se szolgálo lyányát,</l>
                    <l xml:id="l.32">Se barmát, semmi marháját.</l>
                </lg>
                <lg xml:id="lg.9">
                    <fw corresp="Szenci Molnár Albert" type="lg">9 </fw>
                    <l xml:id="l.33">Uram hatalmas az te igéd,</l>
                    <l xml:id="l.34">Még zengésétis rettegjüc,</l>
                    <l xml:id="l.35">Irgalmodbol velünc ezt tegyed,</l>
                    <l xml:id="l.36">Hogy akaratodat tegyüc.</l>
                </lg>
            </div>
        </body>
