    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 95.</fw>
                <head type="title">XCV. SOLTAR. T. B. 24.62.111.</head>
                <head type="sub">Intés az Isteni diczéretre, és szófogadásra.</head>
                <head type="notation">Az XXIIII. Soltar notajára.</head>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">JErtec örvendgyünc mindnyájan</l>
                    <l xml:id="l.2">Az Urban mi kösziklánkban,</l>
                    <l xml:id="l.3">Vigadozzunc szép énekeckel.</l>
                    <l xml:id="l.4">Mennyünc szine eleiben,</l>
                    <l xml:id="l.5">Es magaztallyuc kegyessen</l>
                    <l xml:id="l.6"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">Örvendetes diczéreteckel.</lem>
                            <rdg wit="#W1" xml:id="rdg.1">diczérettekkel</rdg>
                            <rdg wit="#W2" xml:id="rdg.2">diczéretteckel</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.7">Mert ö az hatalmas Isten,</l>
                    <l xml:id="l.8"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Es nagy
                                királyminden<corr>e</corr>ken,</lem>
                            <rdg wit="#W1" xml:id="rdg.3">mindeneken</rdg>
                            <rdg wit="#W2 #W3" xml:id="rdg.4">mindenéken</rdg>
                        </app></l>
                    <l xml:id="l.9">Ö az fejedelmeknec Ura,</l>
                    <l xml:id="l.10">Ez egesz földnec mélségét</l>
                    <l xml:id="l.11">Es az hegyeknec erejét,</l>
                    <l xml:id="l.12"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Hatalmas kezében foglallya.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.5">kezébe</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.13">Övé az tenger, az mellyet</l>
                    <l xml:id="l.14">Erös kezével teremtet,</l>
                    <l xml:id="l.15">Az száraztis ö szerzé szépen,</l>
                    <l xml:id="l.16">Az Urnac jer imátkozzunc,</l>
                    <l xml:id="l.17">Néki térdet fejet haytsunc</l>
                    <l xml:id="l.18"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Az ki minket teremtett bölczen.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.6">münket</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.4">
                    <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                    <l xml:id="l.19">Mert ö Istenünc és Urunc,</l>
                    <l xml:id="l.20">Mi pedig juhai vagyunc,</l>
                    <l xml:id="l.21">Ö legeltet minket, mint nyáyját.</l>
                    <l xml:id="l.22">Lágyitsátoc sziveteket,</l>
                    <l xml:id="l.23">Hogy ha hivand ma titeket,</l>
                    <l xml:id="l.24">Legottan hogy hallyátoc szavát:</l>
                </lg>
                <lg xml:id="lg.5">
                    <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                    <l xml:id="l.25">Mint az Meribánál régen,</l>
                    <l xml:id="l.26">Es az Massa puszta helyén,</l>
                    <l xml:id="l.27">Mond Isten: Az népec mint jártac,</l>
                    <l xml:id="l.28">Holott az ti eleitec</l>
                    <l xml:id="l.29"><app xml:id="app.5">
                            <lem wit="#KK" xml:id="lem.5">Engemet megkisér<corr>t</corr>öttec,</lem>
                            <rdg wit="#W1" xml:id="rdg.7">meg kisértöttek</rdg>
                            <rdg wit="#W2" xml:id="rdg.8">megkisértöttec</rdg>
                            <rdg wit="#W3" xml:id="rdg.9">megkisérttöttec</rdg>
                        </app></l>
                    <l xml:id="l.30"><app xml:id="app.6">
                            <lem wit="#KK" xml:id="lem.6">Es czuda
                                dolgaimatl<corr>á</corr>ttác.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.10">láttác</rdg>
                            <rdg wit="#W3" xml:id="rdg.11">lattác</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.6">
                    <fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
                    <l xml:id="l.31">Hól negyven esztendeiglen</l>
                    <l xml:id="l.32">Engem busitottac szörnyen</l>
                    <l xml:id="l.33">Az ö kemény természeteckel,</l>
                    <l xml:id="l.34">Mondéc végre: Imé ezec</l>
                    <l xml:id="l.35">Gonosságokban elmennec,</l>
                    <l xml:id="l.36">Az én ösvényem nékic nem kel.</l>
                </lg>
                <lg xml:id="lg.7">
                    <fw corresp="Szenci Molnár Albert" type="lg">7 </fw>
                    <l xml:id="l.37">Kiért rájoc megharagvám,</l>
                    <l xml:id="l.38"><app xml:id="app.7">
                            <lem wit="#KK" xml:id="lem.7">Hogy tölöc igy boszszontatám,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.12">boszszontattám</rdg>
                        </app></l>
                    <l xml:id="l.39"><app xml:id="app.8">
                            <lem wit="#KK" xml:id="lem.8">Mivelhogy ök meg
                                nemt<corr>é</corr>rénec,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.13">megnem térénec</rdg>
                            <rdg wit="#W3" xml:id="rdg.14">terénec</rdg>
                        </app></l>
                    <l xml:id="l.40">En megesküvém nékiec,</l>
                    <l xml:id="l.41">Hogy soha öröcké ezec</l>
                    <l xml:id="l.42"><app xml:id="app.9">
                            <lem wit="#KK" xml:id="lem.9">Nyugodalmomba bé nem mennec.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.15">Nyugodalmomban</rdg>
                        </app></l>
                </lg>
            </div>
