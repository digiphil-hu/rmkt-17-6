    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 67.</fw>
                <head type="title">LXVII. SOLTAR. T. B. 33.</head>
                <head type="sub">Könyörgése az Izraelitaknac, az Istennec kegyelmes vélec
                    lakozásaért.</head>
                <head type="notation">Az XXXIII. Soltar notajara.</head>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">UR Isten áldgy meg jovoltodbol,</l>
                    <l xml:id="l.2">Es kegyessen fordully hozzánc, : / :</l>
                    <l xml:id="l.3">Otalmazz meg minden gonosztol,</l>
                    <l xml:id="l.4">Szent szinedet fordits reánc,</l>
                    <l rend="level_1" xml:id="l.5"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">Hogy ez földen minden</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.1">földön</rdg>
                        </app></l>
                    <l xml:id="l.6">Megesmerje szépen</l>
                    <l xml:id="l.7">Az te utadat,</l>
                    <l xml:id="l.8"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Es az pog<corr>á</corr>ny népec</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.2">pogány</rdg>
                            <rdg wit="#W3" xml:id="rdg.3">pogany</rdg>
                        </app></l>
                    <l xml:id="l.9">Téged tisztellyenec</l>
                    <l xml:id="l.10">Megtartójokat.</l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.11"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Es ackoron dicz<corr>é</corr>rnec
                                téged,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.4">diczérnec</rdg>
                            <rdg wit="#W3" xml:id="rdg.5">diczernec</rdg>
                        </app></l>
                    <l xml:id="l.12">Diczérnec téged az népec, : / :</l>
                    <l xml:id="l.13">Nagy tisztességet tesznec néked,</l>
                    <l xml:id="l.14">Az pogányokis örülnec,</l>
                    <l rend="level_1" xml:id="l.15">Midön mindeneket</l>
                    <l xml:id="l.16">Es az pogány népet</l>
                    <l xml:id="l.17">Szent igassággal</l>
                    <l xml:id="l.18"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Birod es itéled</lem>
                            <rdg wit="#W2" xml:id="rdg.6">és</rdg>
                        </app></l>
                    <l xml:id="l.19">Es jóra vezérled</l>
                    <l xml:id="l.20">Nagy hatalmaddal.</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.21">Diczérjen téged minden nemzet,</l>
                    <l xml:id="l.22">Ur Isten téged diczérjen, : / :</l>
                    <l xml:id="l.23">Az föld teremtsen bev gyümölczet,</l>
                    <l xml:id="l.24">Aldgyon meg minket az Isten,</l>
                    <l rend="level_1" xml:id="l.25">Adgya szent malasztyát,</l>
                    <l xml:id="l.26">Nyuytsa áldomását,</l>
                    <l xml:id="l.27">Es ö fölségét</l>
                    <l xml:id="l.28">Féllye és rettegje</l>
                    <l xml:id="l.29">Ez föld kereksége</l>
                    <l xml:id="l.30">Mint ö Istenét.</l>
                </lg>
            </div>
        </body>
