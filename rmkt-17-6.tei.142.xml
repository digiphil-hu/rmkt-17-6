	<text>
		<body>
			<div type="poem" xml:id="d.1" xml:lang="hu">
				<fw type="originalNum">[RMKT XVII/6:] 142.</fw>
				<head type="title">CXLII. SOLTAR. T. B. 100.131.</head>
				<head type="sub"><app xml:id="app.1">
						<lem wit="#KK" xml:id="lem.1">Könyörgése Davidnac <corr>à</corr>
								megszaba<corr>di</corr>tásêrt.</lem>
						<rdg wit="#W1" xml:id="rdg.1">az megszabaditásert</rdg>
						<rdg wit="#W2" xml:id="rdg.2">à megszabaditásért</rdg>
						<rdg wit="#W3" xml:id="rdg.3">á megszabatásêrt</rdg>
					</app></head>
				<head type="notation">Az C. Soltar notajára.</head>
				<lg xml:id="lg.1">
					<fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
					<l xml:id="l.1">EN az Urhoz fölkiáltéc,</l>
					<l xml:id="l.2">Kiáltván néki könyörgéc,</l>
					<l xml:id="l.3">Jelentvén én panaszimat,</l>
					<l xml:id="l.4">Megbeszéllém nyavalyamat.</l>
				</lg>
				<lg xml:id="lg.2">
					<fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
					<l xml:id="l.5">Ha szorongattatic lelkem,</l>
					<l xml:id="l.6">Te utat mutatsz énnékem,</l>
					<l xml:id="l.7">Módot találsz, az melly által</l>
					<l xml:id="l.8">Megszabaduloc jo moddal.</l>
				</lg>
				<lg xml:id="lg.3">
					<fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
					<l xml:id="l.9"><app xml:id="app.2">
							<lem wit="#KK" xml:id="lem.2">Utamra tört mernec
								h<corr>á</corr>nni,</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.4">hánni</rdg>
							<rdg wit="#W3" xml:id="rdg.5">hanni</rdg>
						</app></l>
					<l xml:id="l.10">Az mellyen én szoktam járni,</l>
					<l xml:id="l.11">Tekintec széllel mellölem,</l>
					<l xml:id="l.12"><app xml:id="app.3">
							<lem wit="#KK" xml:id="lem.3">De senki nem esmer engem,</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.6">esmér</rdg>
						</app></l>
				</lg>
				<lg xml:id="lg.4">
					<fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
					<l xml:id="l.13">Bézárlattac utac, aytoc,</l>
					<l xml:id="l.14">Egy felé sem szaladhatoc,</l>
					<l xml:id="l.15">Es ez illyen inségemben</l>
					<l xml:id="l.16">Senki ninczen ki segitsen.</l>
				</lg>
				<lg xml:id="lg.5">
					<fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
					<l xml:id="l.17">Te hozzád kiáltéc Uram,</l>
					<l xml:id="l.18">Mondván: te vagy bizodalmam,</l>
					<l xml:id="l.19">Es egyedül reménségem</l>
					<l xml:id="l.20"><app xml:id="app.4">
							<lem wit="#KK" xml:id="lem.4">E<corr>z</corr> földön czac te vagy
								nékem,</lem>
							<rdg wit="#W1 #W2" xml:id="rdg.7">Ez</rdg>
							<rdg wit="#W3" xml:id="rdg.8">Es</rdg>
						</app></l>
				</lg>
				<lg xml:id="lg.6">
					<fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
					<l xml:id="l.21">Hald meg az én imátságom,</l>
					<l xml:id="l.22">Mert igen sanyargattatom,</l>
					<l xml:id="l.23">Ments meg ellenségim ellen,</l>
					<l xml:id="l.24">Mert erösbec hogynem mint én.</l>
				</lg>
				<lg xml:id="lg.7">
					<fw corresp="Szenci Molnár Albert" type="lg">7 </fw>
					<l xml:id="l.25">Vedd ki fogságbol lelkemet,</l>
					<l xml:id="l.26">Hogy diczérjem szent nevedet,</l>
					<l xml:id="l.27">Ha jol tész velem, az hivec</l>
					<l xml:id="l.28">Ottan engem környülvesznec.</l>
				</lg>
			</div>
		</body>
	</text>
