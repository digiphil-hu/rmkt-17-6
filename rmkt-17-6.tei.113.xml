            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 113.</fw>
                <head type="title">CXIII. SOLTAR. C. M.</head>
                <head type="sub">Diczéreti az Istennec fölséges voltánac és hogy az szegényeket
                    fölemeli.</head>
                <pb facs="264"> </pb>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">AZ Urat ti ö szolgái</l>
                    <l xml:id="l.2"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">Diczérjetec mert
                                <corr>é</corr>rdemli,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.1">érdemli</rdg>
                            <rdg wit="#W3" xml:id="rdg.2">erdemli</rdg>
                        </app></l>
                    <l xml:id="l.3"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Aldgyátoc szent nevét mindnyájan,</lem>
                            <rdg wit="#W1" xml:id="rdg.3">nevit</rdg>
                        </app></l>
                    <l xml:id="l.4"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Diczértesséc szent fölsége</lem>
                            <rdg wit="#W1" xml:id="rdg.4">Diczirtessek</rdg>
                        </app></l>
                    <l xml:id="l.5">Most és öröckön öröcke,</l>
                    <l xml:id="l.6"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Ö szent neve áldasséc tisztan.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.5">tisztán</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.7">Napkelettül enyészetig,</l>
                    <l xml:id="l.8">Aldasséc neve mind végig,</l>
                    <l xml:id="l.9">Mert az Ur az magas mennyekben</l>
                    <l xml:id="l.10">Regnál minden pogányokon</l>
                    <l xml:id="l.11"><app xml:id="app.5">
                            <lem wit="#KK" xml:id="lem.5">Nagy diczö hatalma vagyon,</lem>
                            <rdg wit="#W1" xml:id="rdg.6">düczö</rdg>
                        </app></l>
                    <l xml:id="l.12">Melly föllyül hat az szép egeken.</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.13">De ki vólna hasonlatos</l>
                    <l xml:id="l.14">Ez mi hatalmas Urunkhoz?</l>
                    <l xml:id="l.15">Kinél fölségesb sohol ninczen;</l>
                    <l xml:id="l.16"><app xml:id="app.6">
                            <lem wit="#KK" xml:id="lem.6">Az ki méllyen elalâ néz</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.7">mélyen</rdg>
                        </app></l>
                    <l xml:id="l.17">Mindenre valami hól lesz</l>
                    <l xml:id="l.18">Itt ez földön, és fön az égben.</l>
                </lg>
                <lg xml:id="lg.4">
                    <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                    <l xml:id="l.19">Az szegént porbol fölveszi</l>
                    <l xml:id="l.20">Es az sárbol fölemeli,</l>
                    <l xml:id="l.21">Allapattyát fölmagasztalvan,</l>
                    <l xml:id="l.22">Fölülteti végezetre</l>
                    <l xml:id="l.23">Az nagy fejedelmec köze,</l>
                    <l xml:id="l.24">Az ö népe közt hazájában.</l>
                </lg>
                <lg xml:id="lg.5">
                    <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                    <l xml:id="l.25"><app xml:id="app.7">
                            <lem wit="#KK" xml:id="lem.7">Az aszszony
                                szomoruság<corr>á</corr>t</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.8">szomorusagát</rdg>
                            <rdg wit="#W3" xml:id="rdg.9">szomoruságat</rdg>
                        </app></l>
                    <l xml:id="l.26">Oe magtalansága miát,</l>
                    <l xml:id="l.27">Bodog vigasságra forditya,</l>
                    <l xml:id="l.28"><app xml:id="app.8">
                            <lem wit="#KK" xml:id="lem.8">Gyermekec annyává teszi,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.10">tészi</rdg>
                        </app></l>
                    <l xml:id="l.29">Es szép fiackal szereti,</l>
                    <l xml:id="l.30">Házát gyümölczel szaporitya.</l>
                </lg>
            </div>
        </body>
