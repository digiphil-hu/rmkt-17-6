        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/6:] 132.</fw>
                <head type="title">CXXXII. SOLTAR. T. B.</head>
                <head type="sub">Könyörgés az Istennec jelenléteért, az Jerusalemi Isteni
                    szolgalatban, és kösségi igazgatásban.</head>
                <pb facs="302"/>
                <lg xml:id="lg.1">
                    <fw corresp="Szenci Molnár Albert" type="lg">1 </fw>
                    <l xml:id="l.1">EMlekezzél meg Ur Isten</l>
                    <l xml:id="l.2"><app xml:id="app.1">
                            <lem wit="#KK" xml:id="lem.1">Davidrol és inségéröl,</lem>
                            <rdg wit="#W1" xml:id="rdg.1">inségiröl</rdg>
                        </app></l>
                    <l xml:id="l.3"><app xml:id="app.2">
                            <lem wit="#KK" xml:id="lem.2">Ki néked meg <corr>e</corr>skütt
                                szivböl,</lem>
                            <rdg wit="#W1" xml:id="rdg.2">esküt</rdg>
                            <rdg wit="#W2" xml:id="rdg.3">eskütt</rdg>
                            <rdg wit="#W3" xml:id="rdg.4">éskütt</rdg>
                        </app></l>
                    <l xml:id="l.4"><app xml:id="app.3">
                            <lem wit="#KK" xml:id="lem.3">Es fogad<corr>á</corr>st tött
                                hivségben</lem>
                            <rdg wit="#W1 #W3" xml:id="rdg.5">fogadast</rdg>
                            <rdg wit="#W2" xml:id="rdg.6">fogadást</rdg>
                        </app></l>
                    <l xml:id="l.5">Az Urnac, kit Jacob tisztöl.</l>
                </lg>
                <lg xml:id="lg.2">
                    <fw corresp="Szenci Molnár Albert" type="lg">2 </fw>
                    <l xml:id="l.6">Ezt (ugymond) én fölfogadom,</l>
                    <l xml:id="l.7"><app xml:id="app.4">
                            <lem wit="#KK" xml:id="lem.4">Hogy bé n<corr>e</corr>m megyec
                                házamban,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.7">nem</rdg>
                            <rdg wit="#W3" xml:id="rdg.8">ném</rdg>
                        </app></l>
                    <l xml:id="l.8">Sem le nem fekszem ágyamban,</l>
                    <l xml:id="l.9">Szemeimet bé sem hunyom,</l>
                    <l xml:id="l.10">Minden álmom hátra hagyván:</l>
                </lg>
                <lg xml:id="lg.3">
                    <fw corresp="Szenci Molnár Albert" type="lg">3 </fw>
                    <l xml:id="l.11">Nyugalmam addig nem leszen,</l>
                    <l xml:id="l.12">Miglen helyet nem keresec,</l>
                    <l xml:id="l.13">Az Jacob nagy Istenénec,</l>
                    <l xml:id="l.14">Holott sátort szerzec szépen,</l>
                    <l xml:id="l.15"><app xml:id="app.5">
                            <lem wit="#KK" xml:id="lem.5">Hol diczösége lakozzéc.</lem>
                            <rdg wit="#W1" xml:id="rdg.9">düczösége</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.4">
                    <fw corresp="Szenci Molnár Albert" type="lg">4 </fw>
                    <l xml:id="l.16">Imé ezt nyilván halottuc,</l>
                    <l xml:id="l.17">Hogy az Ephratához néked</l>
                    <l xml:id="l.18">Kiválképpen vagyon kedved,</l>
                    <l xml:id="l.19">Az helyet mi megtaláltuc</l>
                    <l xml:id="l.20">Az mezön az erdö mellet.</l>
                </lg>
                <lg xml:id="lg.5">
                    <fw corresp="Szenci Molnár Albert" type="lg">5 </fw>
                    <l xml:id="l.21">Mi fölmegyünc ez szent helyre</l>
                    <l xml:id="l.22">Es te sámoly széked elöt</l>
                    <l xml:id="l.23">Meghaytyuc az mi térdünköt,</l>
                    <l xml:id="l.24">Uram kelly fel, jöy el ide,</l>
                    <l xml:id="l.25"><app xml:id="app.6">
                            <lem wit="#KK" xml:id="lem.6">Eröd ládájával eggyöt.</lem>
                            <rdg wit="#W1" xml:id="rdg.10">eggyüt</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.6">
                    <fw corresp="Szenci Molnár Albert" type="lg">6 </fw>
                    <l xml:id="l.26">Igasságnac ruháiban</l>
                    <l xml:id="l.27">Papjaid fölöltözzenec,</l>
                    <l xml:id="l.28"><app xml:id="app.7">
                            <lem wit="#KK" xml:id="lem.7">Engedgy ö<corr>r</corr>ömet
                                szentidnec,</lem>
                            <rdg wit="#W1" xml:id="rdg.11">örömet</rdg>
                            <rdg wit="#W2 #W3" xml:id="rdg.12">ötömet</rdg>
                        </app></l>
                    <l xml:id="l.29"><app xml:id="app.8">
                            <lem wit="#KK" xml:id="lem.8">Kenöttidet tarts meg
                                <corr>b</corr>átran,</lem>
                            <rdg wit="#W1 #W2 #W3" xml:id="rdg.13">megbátran</rdg>
                        </app></l>
                    <l xml:id="l.30">Az David néked kellesséc.</l>
                </lg>
                <lg xml:id="lg.7">
                    <metamark function="pause"/>
                    <fw corresp="Szenci Molnár Albert" type="lg"><hi rend="super">*</hi><hi
                            rend="sub">*</hi><hi rend="super">*</hi> 7 </fw>
                    <l xml:id="l.31">Az Davidnac az Ur Isten</l>
                    <l xml:id="l.32">Megeskütt mint szolgájánac,</l>
                    <l xml:id="l.33">Es ö bizvást hihet annac:</l>
                    <l xml:id="l.34">Im az te nemzetedböl én</l>
                    <l xml:id="l.35">Székedre királt állatoc.</l>
                </lg>
                <lg xml:id="lg.8">
                    <fw corresp="Szenci Molnár Albert" type="lg">8 </fw>
                    <l xml:id="l.36">Es osztán az te gyermekid</l>
                    <l xml:id="l.37">Megtartyác én kötésemet</l>
                    <l xml:id="l.38">Es bizonságtételimet,</l>
                    <l xml:id="l.39">Es öröcké az te széked</l>
                    <l xml:id="l.40"><app xml:id="app.9">
                            <lem wit="#KK" xml:id="lem.9">Megülic mint ör<corr>ö</corr>kjöket</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.14">örökjöket</rdg>
                            <rdg wit="#W3" xml:id="rdg.15">öröckjöket</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.9">
                    <fw corresp="Szenci Molnár Albert" type="lg">9 </fw>
                    <l xml:id="l.41">Mert az Siont az Ur Isten</l>
                    <l xml:id="l.42"><app xml:id="app.10">
                            <lem wit="#KK" xml:id="lem.10">Vál<corr>a</corr>sztá lako
                                helyénec,</lem>
                            <rdg wit="#W1" xml:id="rdg.16">Választá</rdg>
                            <rdg wit="#W2 #W3" xml:id="rdg.17">Válásztá</rdg>
                        </app></l>
                    <l xml:id="l.43"><app xml:id="app.11">
                            <lem wit="#KK" xml:id="lem.11">Mondván ez hely kell kedvemnec,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.18">helly</rdg>
                        </app></l>
                    <l xml:id="l.44"><app xml:id="app.12">
                            <lem wit="#KK" xml:id="lem.12">Itt lakom minden idöben,</lem>
                            <rdg wit="#W1" xml:id="rdg.19">üdöben</rdg>
                        </app></l>
                    <l xml:id="l.45"><app xml:id="app.13">
                            <lem wit="#KK" xml:id="lem.13">Ez helye czendességemnec.</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.20">hellye</rdg>
                        </app></l>
                </lg>
                <lg xml:id="lg.10">
                    <fw corresp="Szenci Molnár Albert" type="lg">10 </fw>
                    <l xml:id="l.46">En öket megelégitem,</l>
                    <l xml:id="l.47"><app xml:id="app.14">
                            <lem wit="#KK" xml:id="lem.14">K<corr>e</corr>nyért adoc az
                                szegénnec,</lem>
                            <rdg wit="#W1" xml:id="rdg.21">Kénjert</rdg>
                            <rdg wit="#W2" xml:id="rdg.22">Kényert</rdg>
                            <rdg wit="#W3" xml:id="rdg.23">Kényért</rdg>
                        </app></l>
                    <l xml:id="l.48"><app xml:id="app.15">
                            <lem wit="#KK" xml:id="lem.15">Ruhájában idvösségnec</lem>
                            <rdg wit="#W1" xml:id="rdg.24">üdvösségnec</rdg>
                        </app></l>
                    <l xml:id="l.49">Papjait fölöltöztetem,</l>
                    <l xml:id="l.50">Oerömöc lesz az hiveknec.</l>
                </lg>
                <lg xml:id="lg.11">
                    <fw corresp="Szenci Molnár Albert" type="lg">11 </fw>
                    <l xml:id="l.51">Fölnevelem én egy szarvát</l>
                    <l xml:id="l.52">Az én szolgámnac Davidnac,</l>
                    <l xml:id="l.53">Kit én fölkentem magamnac,</l>
                    <l xml:id="l.54"><app xml:id="app.16">
                            <lem wit="#KK" xml:id="lem.16">Fölkészitöttem
                                lámpas<corr>á</corr>t,</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.25">lámpasát</rdg>
                            <rdg wit="#W3" xml:id="rdg.26">lámpasat</rdg>
                        </app></l>
                    <l xml:id="l.55">Hogy lássa világát annac.</l>
                </lg>
                <lg xml:id="lg.12">
                    <fw corresp="Szenci Molnár Albert" type="lg">12 </fw>
                    <l xml:id="l.56"><app xml:id="app.17">
                            <lem wit="#KK" xml:id="lem.17">De viszont ö ellenségét</lem>
                            <rdg wit="#W1 #W2" xml:id="rdg.27">ellenségit</rdg>
                        </app></l>
                    <l xml:id="l.57">Oeltöztetem nagy szégyenben,</l>
                    <l xml:id="l.58">Midön az koronát fején</l>
                    <l xml:id="l.59"><app xml:id="app.18">
                            <lem wit="#KK" xml:id="lem.18">Megláttyác, és diczöségét</lem>
                            <rdg wit="#W1" xml:id="rdg.28">düczöségit</rdg>
                            <rdg wit="#W2" xml:id="rdg.29">diczöségit</rdg>
                        </app></l>
                    <l xml:id="l.60">Hogy megvirágzic ékessen.</l>
                </lg>
            </div>
        </body>
